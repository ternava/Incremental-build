--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,sparc
--enable-match-finders=bt2,bt4
--enable-checks=crc32,crc64
--enable-small
--enable-threads=yes
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-unsafe-type-punning
--enable-static
--disable-option-checking
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-scripts
--disable-doc
--disable-dependency-tracking
--disable-largefile
