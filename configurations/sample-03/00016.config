--enable-debug
--enable-encoders=lzma1,lzma2,x86,armthumb,sparc,arm,ia64,powerpc,delta
--enable-decoders=lzma1,lzma2
--enable-match-finders=bt2,bt3,bt4
--enable-checks=crc32,sha256
--enable-threads=posix
--enable-sandbox=auto
--enable-silent-rules
--enable-fast-install
--enable-static
