--enable-encoders=lzma1,delta
--enable-decoders=lzma1,delta
--enable-match-finders=bt4
--enable-checks=crc32,sha256
--enable-small
--enable-threads=posix
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--disable-lzmadec
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-libtool-lock
--disable-nls
