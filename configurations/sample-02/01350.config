--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1,armthumb
--enable-match-finders=hc4,bt2,bt4
--enable-checks=crc32
--enable-threads=no
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--disable-assembler
--disable-lzmainfo
--disable-scripts
--disable-doc
--disable-rpath
--disable-largefile
