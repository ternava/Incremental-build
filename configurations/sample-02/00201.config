--enable-encoders=lzma1,sparc
--enable-decoders=lzma1
--enable-match-finders=bt3
--enable-checks=crc32,sha256
--enable-small
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=no
--enable-fast-install
--enable-unsafe-type-punning
--disable-assembler
--disable-lzmadec
--disable-lzmainfo
--disable-scripts
--disable-silent-rules
--disable-dependency-tracking
--disable-nls
--disable-largefile
