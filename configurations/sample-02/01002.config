--enable-debug
--enable-encoders=lzma1,arm
--enable-decoders=lzma1,x86
--enable-match-finders=hc3,bt3,bt4
--enable-checks=crc32,crc64
--enable-shared=no
--enable-sandbox=capsicum
--enable-silent-rules
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-silent-rules
--disable-nls
--disable-rpath
