--enable-debug
--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,ia64
--enable-match-finders=hc4,bt2,bt3,bt4
--enable-small
--enable-threads=posix
--enable-shared=no
--enable-sandbox=auto
--enable-silent-rules
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-lzma-links
--disable-scripts
--disable-nls
