--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1,delta
--enable-match-finders=bt3,bt4
--enable-checks=crc32,sha256
--enable-threads=no
--enable-assume-ram=128
--enable-symbol-versions
--enable-sandbox=auto
--enable-unaligned-access
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-doc
--disable-silent-rules
--disable-largefile
