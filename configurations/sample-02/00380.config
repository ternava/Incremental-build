--enable-debug
--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1,arm
--enable-match-finders=hc4,hc3,bt2,bt3,bt4
--enable-checks=crc32,sha256
--enable-small
--enable-threads=posix
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=no
--enable-silent-rules
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-xzdec
--disable-lzmadec
--disable-scripts
--disable-nls
--disable-largefile
