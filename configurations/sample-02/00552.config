--enable-encoders=lzma1,arm
--enable-decoders=lzma1
--enable-match-finders=hc3,bt3,bt4
--enable-checks=crc32,sha256
--enable-threads=yes
--enable-assume-ram=128
--enable-sandbox=auto
--enable-dependency-tracking
--enable-fast-install
--enable-unaligned-access
--disable-option-checking
--disable-doc
--disable-silent-rules
--disable-libtool-lock
--disable-nls
--disable-rpath
