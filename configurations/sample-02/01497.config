--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1,ia64
--enable-match-finders=hc4,bt4
--enable-checks=crc32,crc64,sha256
--enable-small
--enable-threads=posix
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-fast-install
--enable-unaligned-access
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-lzma-links
--disable-scripts
--disable-silent-rules
--disable-rpath
--disable-largefile
