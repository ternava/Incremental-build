--enable-debug
--enable-encoders=lzma1,arm
--enable-decoders=lzma1,x86
--enable-match-finders=bt3
--enable-checks=crc32,sha256
--enable-threads=yes
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-dependency-tracking
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-scripts
--disable-doc
--disable-silent-rules
--disable-nls
--disable-largefile
