--enable-encoders=lzma1,arm
--enable-decoders=lzma1,x86
--enable-match-finders=bt2
--enable-checks=crc32,crc64,sha256
--enable-small
--enable-threads=yes
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-dependency-tracking
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-lzmadec
--disable-doc
--disable-silent-rules
--disable-libtool-lock
--disable-nls
--disable-largefile
