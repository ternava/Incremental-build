--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1,arm
--enable-match-finders=hc3,bt2,bt4
--enable-checks=crc32,crc64,sha256
--enable-small
--enable-threads=yes
--enable-assume-ram=128
--enable-symbol-versions
--enable-sandbox=no
--enable-fast-install
--disable-option-checking
--disable-lzmadec
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-silent-rules
