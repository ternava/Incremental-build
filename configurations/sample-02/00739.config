--enable-encoders=lzma1,x86
--enable-decoders=lzma1,arm
--enable-match-finders=hc4,hc3,bt2
--enable-checks=crc32,sha256
--enable-threads=yes
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-silent-rules
--enable-dependency-tracking
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-lzmadec
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-largefile
