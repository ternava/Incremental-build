--enable-debug
--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,x86
--enable-match-finders=bt2,bt3,bt4
--enable-small
--enable-threads=yes
--enable-shared=no
--enable-sandbox=auto
--enable-silent-rules
--enable-unaligned-access
--enable-static
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-scripts
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
