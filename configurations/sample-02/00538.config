--enable-debug
--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1,ia64
--enable-match-finders=bt2
--enable-checks=crc32,crc64
--enable-threads=no
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=no
--enable-silent-rules
--enable-dependency-tracking
--enable-unaligned-access
--disable-option-checking
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-silent-rules
--disable-rpath
--disable-largefile
