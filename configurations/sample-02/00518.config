--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1,x86
--enable-match-finders=hc4,bt3,bt4
--enable-checks=crc32,crc64,sha256
--enable-small
--enable-threads=posix
--enable-assume-ram=128
--enable-sandbox=capsicum
--enable-silent-rules
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-option-checking
--disable-lzmadec
--disable-lzmainfo
--disable-silent-rules
--disable-libtool-lock
--disable-rpath
--disable-largefile
