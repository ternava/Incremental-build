--enable-debug
--enable-encoders=lzma1
--enable-decoders=lzma1,ia64
--enable-match-finders=bt3
--enable-checks=crc32
--enable-small
--enable-shared=no
--enable-sandbox=auto
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-lzmadec
--disable-lzmainfo
--disable-dependency-tracking
--disable-libtool-lock
--disable-largefile
