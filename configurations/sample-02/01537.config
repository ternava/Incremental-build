--enable-encoders=lzma1,ia64
--enable-decoders=lzma1,lzma2
--enable-match-finders=hc4,bt3
--enable-checks=crc32,crc64,sha256
--enable-threads=yes
--enable-shared=no
--enable-symbol-versions
