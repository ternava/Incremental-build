--enable-debug
--enable-encoders=lzma1,ia64
--enable-decoders=lzma1,lzma2
--enable-match-finders=bt2,bt4
--enable-checks=crc32
--enable-threads=yes
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-static
--enable-fast-install=no
--disable-lzmadec
--disable-scripts
--disable-doc
--disable-silent-rules
--disable-dependency-tracking
--disable-nls
