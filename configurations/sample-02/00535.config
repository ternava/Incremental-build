--enable-debug
--enable-encoders=lzma1,sparc
--enable-decoders=lzma1
--enable-match-finders=hc4,bt3,bt4
--enable-checks=crc32,sha256
--enable-small
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install
--disable-option-checking
--disable-lzmadec
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-nls
--disable-rpath
--disable-largefile
