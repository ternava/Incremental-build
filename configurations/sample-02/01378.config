--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,arm
--enable-match-finders=hc4,bt3,bt4
--enable-small
--enable-threads=yes
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-fast-install=no
--disable-option-checking
--disable-xzdec
--disable-silent-rules
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
