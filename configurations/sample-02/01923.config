--enable-debug
--enable-encoders=lzma1,arm
--enable-decoders=lzma1,powerpc
--enable-match-finders=hc4,bt2,bt4
--enable-checks=crc32,crc64
--enable-threads=posix
--enable-assume-ram=128
--enable-shared=no
--enable-sandbox=capsicum
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-lzmainfo
--disable-scripts
--disable-doc
--disable-dependency-tracking
--disable-rpath
--disable-largefile
