--enable-encoders=lzma1
--enable-decoders=lzma1,sparc
--enable-match-finders=bt3
--enable-checks=crc32,crc64,sha256
--enable-small
--enable-threads=yes
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-unsafe-type-punning
--enable-static
--disable-option-checking
--disable-assembler
--disable-lzmadec
--disable-scripts
--disable-doc
--disable-dependency-tracking
--disable-nls
--disable-largefile
