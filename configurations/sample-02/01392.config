--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1,delta
--enable-match-finders=hc4,hc3,bt3
--enable-checks=crc32,crc64,sha256
--enable-threads=no
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-unsafe-type-punning
--enable-static
--disable-option-checking
--disable-assembler
--disable-lzmadec
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
