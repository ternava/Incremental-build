--enable-debug
--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,ia64
--enable-match-finders=hc3,hc4
--enable-checks=crc32,crc64
--enable-small
--enable-threads=posix
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-lzma-links
--disable-scripts
--disable-dependency-tracking
--disable-rpath
--disable-largefile
