--enable-debug
--enable-encoders=lzma1
--enable-decoders=lzma1,powerpc
--enable-match-finders=hc4,bt2,bt3,bt4
--enable-checks=crc32,sha256
--enable-small
--enable-threads=yes
--enable-shared=no
--enable-sandbox=no
--enable-silent-rules
--enable-fast-install
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-lzmainfo
--disable-scripts
--disable-doc
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
