--enable-debug
--enable-encoders=lzma1,arm
--enable-decoders=lzma1,lzma2
--enable-match-finders=hc4,bt3
--enable-checks=crc32,sha256
--enable-threads=no
--enable-assume-ram=128
--enable-shared=no
--enable-dependency-tracking
--enable-fast-install
--enable-unsafe-type-punning
--enable-static
--disable-option-checking
--disable-assembler
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-nls
--disable-rpath
