--enable-debug
--enable-encoders=lzma1,delta
--enable-decoders=lzma1,ia64
--enable-match-finders=bt2,bt3
--enable-checks=crc32,crc64
--enable-threads=yes
--enable-shared=no
--enable-sandbox=no
--enable-dependency-tracking
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-doc
--disable-libtool-lock
--disable-rpath
