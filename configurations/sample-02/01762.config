--enable-debug
--enable-encoders=lzma1,arm
--enable-decoders=lzma1,ia64
--enable-match-finders=hc3,hc4
--enable-checks=crc32,crc64
--enable-threads=no
--enable-shared=no
--enable-sandbox=no
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-scripts
--disable-libtool-lock
--disable-nls
--disable-largefile
