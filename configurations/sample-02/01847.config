--enable-encoders=lzma1,x86
--enable-decoders=lzma1,ia64
--enable-match-finders=hc3,bt2
--enable-checks=crc32,crc64,sha256
--enable-small
--enable-threads=posix
--enable-sandbox=no
--enable-silent-rules
--enable-fast-install
--enable-unsafe-type-punning
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-libtool-lock
--disable-rpath
