--enable-encoders=lzma1
--enable-decoders=lzma1,lzma2
--enable-match-finders=hc4
--enable-checks=crc32,crc64
--enable-threads=yes
--enable-symbol-versions
--enable-sandbox=no
--enable-silent-rules
--enable-fast-install
--enable-unaligned-access
--disable-assembler
--disable-lzmadec
--disable-lzmainfo
--disable-scripts
--disable-libtool-lock
--disable-rpath
--disable-largefile
