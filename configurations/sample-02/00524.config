--enable-debug
--enable-encoders=lzma1
--enable-decoders=lzma1,ia64
--enable-match-finders=bt2,bt3,bt4
--enable-checks=crc32,crc64,sha256
--enable-threads=yes
--enable-silent-rules
--enable-fast-install
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-assembler
--disable-lzmadec
--disable-scripts
--disable-doc
--disable-dependency-tracking
--disable-rpath
