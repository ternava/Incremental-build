--enable-encoders=lzma1,x86
--enable-decoders=lzma1,lzma2
--enable-match-finders=hc4,bt3,bt4
--enable-checks=crc32,crc64
--enable-small
--enable-threads=yes
--enable-assume-ram=128
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-silent-rules
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-lzmadec
--disable-lzmainfo
--disable-scripts
--disable-libtool-lock
--disable-rpath
--disable-largefile
