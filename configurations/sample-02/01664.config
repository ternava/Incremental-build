--enable-debug
--enable-encoders=lzma1
--enable-decoders=lzma1,armthumb
--enable-match-finders=hc4,hc3,bt4
--enable-checks=crc32
--enable-assume-ram=128
--enable-sandbox=auto
--enable-silent-rules
--enable-fast-install
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-xzdec
--disable-lzmadec
--disable-doc
--disable-silent-rules
--disable-dependency-tracking
--disable-libtool-lock
--disable-rpath
