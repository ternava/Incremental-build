--enable-decoders=lzma1,sparc
--enable-match-finders=bt3,bt4
--enable-checks=crc32,crc64
--enable-small
--enable-threads=yes
--enable-assume-ram=128
--enable-sandbox=capsicum
--enable-dependency-tracking
--enable-unsafe-type-punning
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-doc
--disable-silent-rules
--disable-nls
--disable-rpath
--disable-largefile
