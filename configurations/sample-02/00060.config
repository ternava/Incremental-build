--enable-debug
--enable-encoders=lzma1,lzma2,x86,armthumb,sparc,arm,ia64,powerpc,delta
--enable-decoders=lzma1,ia64
--enable-match-finders=hc4
--enable-checks=crc32,sha256
--enable-threads=no
--enable-sandbox=auto
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-lzmainfo
--disable-lzma-links
--disable-libtool-lock
--disable-nls
--disable-rpath
