--enable-debug
--enable-encoders=lzma1,ia64
--enable-decoders=lzma1,sparc
--enable-match-finders=bt2,bt4
--enable-checks=crc32,sha256
--enable-assume-ram=128
--enable-symbol-versions
--enable-silent-rules
--enable-fast-install
--enable-unsafe-type-punning
--enable-static
--disable-assembler
--disable-xzdec
--disable-lzma-links
--disable-doc
--disable-nls
--disable-rpath
--disable-largefile
