--enable-debug
--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1,lzma2
--enable-match-finders=hc3,bt2,bt3
--enable-checks=crc32,crc64,sha256
--enable-small
--enable-threads=posix
--enable-assume-ram=128
--enable-sandbox=no
--enable-dependency-tracking
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--disable-xzdec
--disable-lzma-links
--disable-doc
