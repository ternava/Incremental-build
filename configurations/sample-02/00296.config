--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,lzma2,delta,x86,sparc,ia64,armthumb,arm,powerpc
--enable-match-finders=hc4,hc3,bt4
--enable-checks=crc32,crc64,sha256
--enable-threads=no
--enable-sandbox=no
--enable-unaligned-access
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-scripts
--disable-silent-rules
--disable-libtool-lock
--disable-nls
--disable-largefile
