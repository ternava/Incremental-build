--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,delta
--enable-match-finders=bt2,bt3
--enable-checks=crc32,crc64,sha256
--enable-threads=yes
--enable-shared=no
--enable-dependency-tracking
--enable-fast-install=no
--disable-lzmainfo
--disable-lzma-links
--disable-silent-rules
--disable-nls
--disable-rpath
--disable-largefile
