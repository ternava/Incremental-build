--enable-encoders=lzma1,ia64
--enable-decoders=lzma1,arm
--enable-match-finders=bt2
--enable-checks=crc32,sha256
--enable-threads=posix
--enable-shared=no
--enable-symbol-versions
--enable-silent-rules
--enable-fast-install
--enable-unsafe-type-punning
--enable-static
--disable-option-checking
--disable-xzdec
--disable-libtool-lock
--disable-largefile
