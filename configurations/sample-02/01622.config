--enable-encoders=lzma1
--enable-decoders=lzma1,delta
--enable-match-finders=hc3,bt2
--enable-checks=crc32
--enable-threads=yes
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-lzmadec
--disable-scripts
--disable-doc
