--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1,delta
--enable-match-finders=hc4,bt2
--enable-checks=crc32
--enable-threads=no
--enable-shared=no
--enable-sandbox=capsicum
--enable-silent-rules
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-dependency-tracking
--disable-largefile
