--enable-encoders=lzma1,ia64
--enable-decoders=lzma1
--enable-match-finders=hc4,bt2,bt3
--enable-checks=crc32
--enable-small
--enable-threads=yes
--enable-sandbox=no
--enable-silent-rules
--enable-dependency-tracking
--enable-unsafe-type-punning
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-libtool-lock
--disable-largefile
