--enable-debug
--enable-encoders=lzma1,ia64
--enable-decoders=lzma1,lzma2
--enable-match-finders=hc4,hc3,bt4
--enable-checks=crc32,sha256
--enable-threads=no
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--disable-option-checking
--disable-assembler
--disable-lzmadec
--disable-scripts
--disable-libtool-lock
--disable-nls
--disable-rpath
