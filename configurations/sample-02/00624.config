--enable-encoders=lzma1,arm
--enable-decoders=lzma1
--enable-match-finders=bt2,bt3,bt4
--enable-checks=crc32,crc64
--enable-threads=yes
--enable-assume-ram=128
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-fast-install
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-silent-rules
--disable-dependency-tracking
--disable-rpath
