--enable-encoders=lzma1
--enable-decoders=lzma1
--enable-match-finders=hc3
--enable-checks=crc32,sha256
--enable-small
--enable-assume-ram=128
--enable-symbol-versions
--enable-sandbox=auto
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-assembler
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-rpath
