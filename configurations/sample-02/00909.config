--enable-encoders=lzma1,delta
--enable-decoders=lzma1,delta
--enable-match-finders=hc4,hc3,bt3,bt4
--enable-checks=crc32
--enable-threads=yes
--enable-assume-ram=128
--enable-sandbox=auto
--enable-silent-rules
--enable-fast-install
--enable-unaligned-access
--enable-static
--disable-lzmainfo
--disable-scripts
--disable-doc
--disable-libtool-lock
--disable-rpath
--disable-largefile
