--enable-encoders=lzma1
--enable-decoders=lzma1,ia64
--enable-match-finders=hc3,bt3,bt4
--enable-checks=crc32,sha256
--enable-threads=yes
--enable-assume-ram=128
--enable-shared=no
--enable-sandbox=no
--enable-dependency-tracking
--enable-fast-install
--enable-unsafe-type-punning
--enable-static
--disable-option-checking
--disable-assembler
--disable-doc
--disable-nls
--disable-rpath
--disable-largefile
