--enable-encoders=lzma1,ia64
--enable-decoders=lzma1,delta
--enable-match-finders=hc4,hc3,bt2,bt4
--enable-threads=posix
--enable-sandbox=auto
--enable-silent-rules
--enable-unsafe-type-punning
--disable-option-checking
--disable-lzmadec
--disable-lzma-links
--disable-dependency-tracking
--disable-libtool-lock
--disable-largefile
