--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1,powerpc
--enable-match-finders=hc3,bt2
--enable-checks=crc32,crc64
--enable-threads=no
--enable-shared=no
--enable-sandbox=no
--enable-silent-rules
--enable-fast-install
--enable-unaligned-access
--disable-option-checking
--disable-xzdec
--disable-lzma-links
--disable-doc
--disable-dependency-tracking
--disable-rpath
--disable-largefile
