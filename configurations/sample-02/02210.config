--enable-debug
--enable-encoders=lzma1,delta
--enable-decoders=lzma1,ia64
--enable-match-finders=hc3
--enable-checks=crc32,crc64,sha256
--enable-threads=posix
--enable-symbol-versions
--enable-sandbox=auto
--enable-unaligned-access
--enable-static
--disable-assembler
--disable-lzma-links
--disable-scripts
--disable-silent-rules
