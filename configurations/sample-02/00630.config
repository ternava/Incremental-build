--enable-encoders=lzma1,ia64
--enable-decoders=lzma1,arm
--enable-match-finders=bt2,bt4
--enable-checks=crc32,crc64,sha256
--enable-small
--enable-threads=yes
--enable-assume-ram=128
--enable-sandbox=auto
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-xzdec
--disable-lzma-links
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
