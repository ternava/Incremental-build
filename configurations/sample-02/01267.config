--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,sparc
--enable-match-finders=hc3,bt4
--enable-checks=crc32,crc64
--enable-threads=no
--enable-sandbox=capsicum
--enable-silent-rules
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-xzdec
--disable-lzmadec
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-dependency-tracking
--disable-nls
--disable-rpath
