--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1,x86
--enable-match-finders=bt3,bt4
--enable-checks=crc32
--enable-small
--enable-threads=posix
--enable-shared=no
--enable-sandbox=no
--enable-dependency-tracking
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-libtool-lock
--disable-rpath
--disable-largefile
