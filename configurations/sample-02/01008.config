--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,powerpc
--enable-match-finders=hc3,bt2
--enable-checks=crc32
--enable-threads=no
--enable-shared=no
--enable-sandbox=no
--enable-silent-rules
--enable-unaligned-access
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-doc
--disable-dependency-tracking
--disable-nls
--disable-largefile
