--enable-debug
--enable-encoders=lzma1,ia64
--enable-decoders=lzma1,ia64
--enable-match-finders=bt3,bt4
--enable-checks=crc32,crc64
--enable-threads=no
--enable-assume-ram=128
--enable-shared=no
--enable-sandbox=auto
--enable-silent-rules
--enable-dependency-tracking
--enable-unaligned-access
--enable-fast-install=no
--disable-assembler
--disable-xzdec
--disable-lzmainfo
--disable-lzma-links
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
