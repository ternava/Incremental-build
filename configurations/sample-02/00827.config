--enable-debug
--enable-encoders=lzma1,arm
--enable-decoders=lzma1,ia64
--enable-match-finders=hc3,bt3
--enable-threads=no
--enable-assume-ram=128
--enable-shared=no
--enable-sandbox=auto
--enable-silent-rules
--enable-dependency-tracking
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-silent-rules
