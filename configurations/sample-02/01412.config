--enable-debug
--enable-encoders=lzma1,delta
--enable-decoders=lzma1,x86
--enable-match-finders=hc4,hc3,bt2,bt4
--enable-checks=crc32,crc64
--enable-threads=yes
--enable-assume-ram=128
--enable-shared=no
--enable-silent-rules
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--disable-lzmainfo
--disable-lzma-links
--disable-silent-rules
--disable-nls
