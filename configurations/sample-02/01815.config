--enable-encoders=lzma1,arm
--enable-decoders=lzma1,x86
--enable-match-finders=bt2,bt4
--enable-checks=crc32,crc64
--enable-threads=no
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-static
--enable-fast-install=no
--disable-xzdec
--disable-lzma-links
--disable-doc
--disable-libtool-lock
--disable-nls
--disable-rpath
