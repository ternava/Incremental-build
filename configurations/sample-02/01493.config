--enable-encoders=lzma1,x86
--enable-decoders=lzma1,lzma2
--enable-match-finders=bt2
--enable-checks=crc32
--enable-small
--enable-threads=yes
--enable-sandbox=no
--enable-dependency-tracking
--enable-fast-install
--enable-unsafe-type-punning
--enable-static
--disable-option-checking
--disable-lzmainfo
--disable-lzma-links
--disable-doc
--disable-silent-rules
--disable-nls
--disable-rpath
--disable-largefile
