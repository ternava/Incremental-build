--enable-debug
--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1
--enable-match-finders=bt2,bt4
--enable-checks=crc32
--enable-small
--enable-assume-ram=128
--enable-symbol-versions
--enable-sandbox=no
--enable-silent-rules
--disable-option-checking
--disable-scripts
--disable-libtool-lock
--disable-nls
--disable-largefile
