--enable-debug
--enable-encoders=lzma1,arm
--enable-decoders=lzma1,ia64
--enable-match-finders=hc3,bt3
--enable-checks=crc32,sha256
--enable-small
--enable-threads=yes
--enable-sandbox=auto
--enable-dependency-tracking
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-lzmadec
--disable-scripts
--disable-rpath
