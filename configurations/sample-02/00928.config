--enable-debug
--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,x86
--enable-match-finders=hc4,bt4
--enable-checks=crc32,crc64
--enable-threads=yes
--enable-assume-ram=128
--enable-shared=no
--enable-sandbox=auto
--enable-silent-rules
--enable-fast-install
--enable-unaligned-access
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-lzmainfo
--disable-lzma-links
--disable-doc
--disable-libtool-lock
--disable-largefile
