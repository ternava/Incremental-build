--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1,armthumb
--enable-match-finders=hc4,hc3,bt3,bt4
--enable-checks=crc32,crc64
--enable-small
--enable-threads=posix
--enable-assume-ram=128
--enable-sandbox=no
--enable-dependency-tracking
--enable-unsafe-type-punning
--enable-static
--disable-assembler
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-rpath
