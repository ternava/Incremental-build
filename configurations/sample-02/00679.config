--enable-debug
--enable-encoders=lzma1,lzma2,x86,armthumb,sparc,arm,ia64,powerpc,delta
--enable-decoders=lzma1,delta
--enable-match-finders=hc4,hc3,bt3
--enable-checks=crc32
--enable-threads=yes
--enable-symbol-versions
--enable-sandbox=auto
--enable-fast-install
--enable-unaligned-access
--enable-static
--disable-assembler
--disable-lzmadec
--disable-lzmainfo
--disable-doc
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
