--enable-debug
--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,delta
--enable-match-finders=hc4,hc3,bt3,bt4
--enable-checks=crc32
--enable-small
--enable-threads=posix
--enable-assume-ram=128
--enable-sandbox=capsicum
--enable-dependency-tracking
--enable-fast-install
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-option-checking
--disable-xzdec
--disable-dependency-tracking
--disable-libtool-lock
--disable-rpath
--disable-largefile
