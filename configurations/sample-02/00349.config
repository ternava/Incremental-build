--enable-encoders=lzma1,delta
--enable-decoders=lzma1,arm
--enable-match-finders=bt2
--enable-checks=crc32,sha256
--enable-small
--enable-threads=yes
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-unaligned-access
--enable-unsafe-type-punning
--disable-assembler
--disable-xzdec
--disable-doc
--disable-libtool-lock
--disable-nls
--disable-largefile
