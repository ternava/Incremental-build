--enable-encoders=lzma1,x86
--enable-decoders=lzma1,sparc
--enable-match-finders=hc4,hc3,bt4
--enable-checks=crc32,crc64
--enable-threads=yes
--enable-symbol-versions
--enable-sandbox=no
--enable-silent-rules
--enable-unaligned-access
--enable-unsafe-type-punning
--disable-option-checking
--disable-assembler
--disable-lzmadec
--disable-lzma-links
--disable-largefile
