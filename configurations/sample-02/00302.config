--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,x86
--enable-match-finders=bt4
--enable-checks=crc32,sha256
--enable-small
--enable-threads=yes
--enable-symbol-versions
--enable-sandbox=no
--enable-silent-rules
--enable-fast-install
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-libtool-lock
--disable-nls
