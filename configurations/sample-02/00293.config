--enable-encoders=lzma1
--enable-decoders=lzma1,powerpc
--enable-match-finders=hc4
--enable-checks=crc32,crc64,sha256
--enable-threads=posix
--enable-sandbox=capsicum
--enable-silent-rules
--enable-dependency-tracking
--enable-unaligned-access
--disable-xzdec
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-libtool-lock
--disable-nls
--disable-rpath
