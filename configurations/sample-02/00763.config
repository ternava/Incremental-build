--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1,x86
--enable-match-finders=hc4,bt3,bt4
--enable-checks=crc32
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=no
--enable-dependency-tracking
--enable-fast-install
--enable-unsafe-type-punning
--disable-lzmainfo
--disable-lzma-links
--disable-silent-rules
--disable-dependency-tracking
--disable-libtool-lock
--disable-rpath
--disable-largefile
