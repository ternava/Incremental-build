--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1,sparc
--enable-match-finders=hc4
--enable-checks=crc32,crc64,sha256
--enable-threads=yes
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-unaligned-access
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzma-links
--disable-silent-rules
--disable-nls
--disable-rpath
