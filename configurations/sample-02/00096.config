--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1,ia64
--enable-match-finders=hc3,bt2,bt3
--enable-checks=crc32,crc64
--enable-small
--enable-threads=yes
--enable-assume-ram=128
--enable-sandbox=capsicum
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-assembler
--disable-lzmadec
--disable-lzma-links
--disable-scripts
--disable-libtool-lock
--disable-rpath
--disable-largefile
