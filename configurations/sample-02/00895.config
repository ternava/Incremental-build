--enable-debug
--enable-encoders=lzma1,delta
--enable-decoders=lzma1
--enable-match-finders=hc3,bt2,bt4
--enable-checks=crc32,crc64
--enable-small
--enable-assume-ram=128
--enable-sandbox=auto
--enable-silent-rules
--enable-unaligned-access
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-scripts
--disable-silent-rules
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-rpath
