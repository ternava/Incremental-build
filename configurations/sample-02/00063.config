--enable-debug
--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1,x86
--enable-match-finders=hc4,hc3,bt3
--enable-checks=crc32
--enable-small
--enable-shared=no
--enable-sandbox=no
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-silent-rules
--disable-rpath
--disable-largefile
