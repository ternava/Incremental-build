--enable-debug
--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1
--enable-match-finders=hc4
--enable-checks=crc32
--enable-small
--enable-threads=yes
--enable-shared=no
--enable-sandbox=no
--enable-dependency-tracking
--enable-fast-install=no
--disable-option-checking
--disable-scripts
--disable-doc
--disable-silent-rules
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
