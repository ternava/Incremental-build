--enable-debug
--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,x86
--enable-match-finders=hc4,bt2,bt3
--enable-checks=crc32,crc64,sha256
--enable-threads=posix
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-silent-rules
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-lzma-links
--disable-silent-rules
--disable-dependency-tracking
--disable-largefile
