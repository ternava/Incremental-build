--enable-debug
--enable-encoders=lzma1,delta
--enable-decoders=lzma1,ia64
--enable-match-finders=bt2,bt3
--enable-checks=crc32,crc64,sha256
--enable-threads=yes
--enable-shared=no
--enable-sandbox=capsicum
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install
--enable-unaligned-access
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-lzmainfo
--disable-dependency-tracking
--disable-nls
--disable-rpath
--disable-largefile
