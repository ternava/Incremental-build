--enable-debug
--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1
--enable-match-finders=hc4,hc3,bt2
--enable-checks=crc32
--enable-threads=no
--enable-shared=no
--enable-symbol-versions
--enable-silent-rules
--enable-fast-install=no
--disable-option-checking
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-doc
--disable-libtool-lock
--disable-nls
--disable-largefile
