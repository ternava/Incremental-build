--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,x86
--enable-match-finders=bt3,bt4
--enable-checks=crc32
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=no
--enable-dependency-tracking
--enable-unaligned-access
--disable-lzmadec
--disable-lzma-links
--disable-scripts
--disable-silent-rules
--disable-libtool-lock
--disable-largefile
