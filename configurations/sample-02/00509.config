--enable-encoders=lzma1,delta
--enable-decoders=lzma1,armthumb
--enable-match-finders=hc4,bt4
--enable-checks=crc32
--enable-small
--enable-threads=posix
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-silent-rules
--enable-static
--disable-assembler
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-doc
--disable-libtool-lock
--disable-nls
--disable-largefile
