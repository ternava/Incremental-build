--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,x86
--enable-match-finders=hc4,hc3,bt4
--enable-checks=crc32,crc64
--enable-threads=posix
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-lzmadec
--disable-lzmainfo
--disable-silent-rules
--disable-dependency-tracking
--disable-nls
--disable-rpath
--disable-largefile
