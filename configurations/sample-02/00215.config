--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,armthumb
--enable-match-finders=hc3,bt2
--enable-checks=crc32,crc64
--enable-threads=posix
--enable-assume-ram=128
--enable-sandbox=auto
--enable-dependency-tracking
--enable-fast-install
--enable-fast-install=no
--disable-option-checking
--disable-xzdec
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-silent-rules
--disable-nls
