--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,sparc
--enable-match-finders=hc4,bt3,bt4
--enable-checks=crc32,sha256
--enable-small
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-assembler
--disable-xzdec
--disable-lzmainfo
--disable-lzma-links
--disable-dependency-tracking
--disable-rpath
--disable-largefile
