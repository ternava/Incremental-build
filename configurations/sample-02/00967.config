--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1,x86
--enable-match-finders=bt2,bt4
--enable-checks=crc32,sha256
--enable-small
--enable-threads=yes
--enable-assume-ram=128
--enable-sandbox=no
--enable-unaligned-access
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-lzmadec
--disable-lzmainfo
--disable-libtool-lock
--disable-largefile
