--enable-debug
--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,lzma2
--enable-match-finders=bt4
--enable-checks=crc32
--enable-small
--enable-threads=posix
--enable-assume-ram=128
--enable-symbol-versions
--enable-fast-install
--enable-static
--disable-option-checking
--disable-assembler
--disable-doc
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
