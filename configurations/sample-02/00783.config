--enable-encoders=lzma1,ia64
--enable-decoders=lzma1,lzma2
--enable-match-finders=hc3,hc4
--enable-checks=crc32,crc64
--enable-threads=no
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-silent-rules
--enable-dependency-tracking
--enable-unaligned-access
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-lzmadec
--disable-lzma-links
--disable-rpath
