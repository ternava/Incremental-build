--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1
--enable-match-finders=hc3
--enable-checks=crc32,crc64
--enable-threads=no
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-dependency-tracking
--enable-unaligned-access
--enable-unsafe-type-punning
--disable-assembler
--disable-lzmadec
--disable-scripts
--disable-silent-rules
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
