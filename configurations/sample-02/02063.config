--enable-debug
--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1,x86
--enable-match-finders=bt2,bt3,bt4
--enable-checks=crc32,sha256
--enable-threads=no
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install=no
--disable-assembler
--disable-xzdec
--disable-lzmainfo
--disable-doc
--disable-nls
--disable-rpath
