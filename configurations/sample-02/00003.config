--enable-debug
--enable-encoders=lzma1,arm
--enable-decoders=lzma1,arm
--enable-match-finders=bt3
--enable-checks=crc32,crc64
--enable-assume-ram=128
--enable-shared=no
--enable-sandbox=capsicum
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--disable-option-checking
--disable-assembler
--disable-lzmadec
--disable-lzma-links
--disable-doc
--disable-silent-rules
--disable-nls
--disable-largefile
