--enable-encoders=lzma1,x86
--enable-decoders=lzma1,arm
--enable-match-finders=hc3,hc4
--enable-checks=crc32,crc64,sha256
--enable-threads=yes
--enable-assume-ram=128
--enable-symbol-versions
--enable-sandbox=no
--enable-dependency-tracking
--enable-unaligned-access
--enable-static
--disable-assembler
--disable-xzdec
--disable-lzma-links
--disable-scripts
--disable-nls
--disable-largefile
