--enable-debug
--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,delta
--enable-match-finders=hc4,bt2,bt3,bt4
--enable-checks=crc32,crc64,sha256
--enable-threads=yes
--enable-shared=no
--enable-sandbox=auto
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-lzma-links
--disable-scripts
--disable-silent-rules
--disable-libtool-lock
--disable-rpath
