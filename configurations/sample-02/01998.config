--enable-debug
--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1
--enable-match-finders=bt2,bt3
--enable-checks=crc32,sha256
--enable-small
--enable-threads=yes
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-unaligned-access
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzma-links
--disable-silent-rules
--disable-dependency-tracking
--disable-nls
--disable-rpath
