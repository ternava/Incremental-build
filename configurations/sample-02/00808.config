--enable-debug
--enable-encoders=lzma1,delta
--enable-decoders=lzma1
--enable-match-finders=hc4,hc3,bt3,bt4
--enable-checks=crc32,sha256
--enable-threads=posix
--enable-assume-ram=128
--enable-shared=no
--enable-sandbox=auto
--enable-silent-rules
