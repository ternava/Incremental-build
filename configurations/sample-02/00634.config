--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1,lzma2,delta,x86,sparc,ia64,armthumb,arm,powerpc
--enable-match-finders=hc4,hc3,bt4
--enable-checks=crc32
--enable-small
--enable-assume-ram=128
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-fast-install
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-lzmadec
--disable-doc
--disable-silent-rules
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
