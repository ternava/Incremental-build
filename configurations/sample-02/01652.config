--enable-debug
--enable-encoders=lzma1
--enable-decoders=lzma1,armthumb
--enable-match-finders=hc4,hc3,bt4
--enable-checks=crc32
--enable-threads=no
--enable-assume-ram=128
--enable-sandbox=no
--enable-silent-rules
--enable-dependency-tracking
--enable-unaligned-access
--enable-unsafe-type-punning
--disable-assembler
--disable-lzmadec
--disable-scripts
--disable-silent-rules
--disable-nls
--disable-largefile
