--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,sparc
--enable-match-finders=hc3,bt3,bt4
--enable-checks=crc32,crc64,sha256
--enable-threads=posix
--enable-shared=no
--enable-static
--enable-fast-install=no
--disable-xzdec
--disable-lzma-links
--disable-scripts
--disable-silent-rules
--disable-dependency-tracking
--disable-nls
--disable-rpath
--disable-largefile
