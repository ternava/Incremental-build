--enable-debug
--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1
--enable-match-finders=hc4,hc3,bt4
--enable-checks=crc32,crc64
--enable-threads=posix
--enable-assume-ram=128
--enable-shared=no
--enable-sandbox=capsicum
--enable-silent-rules
--enable-fast-install
--enable-static
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-libtool-lock
--disable-rpath
