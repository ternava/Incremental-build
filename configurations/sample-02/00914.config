--enable-debug
--enable-encoders=lzma1
--enable-decoders=lzma1,delta
--enable-match-finders=hc3,bt2,bt4
--enable-checks=crc32
--enable-threads=no
--enable-shared=no
--enable-symbol-versions
--enable-silent-rules
--enable-unaligned-access
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-lzmainfo
--disable-lzma-links
--disable-doc
--disable-nls
--disable-rpath
