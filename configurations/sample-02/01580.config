--enable-debug
--enable-encoders=lzma1
--enable-decoders=lzma1,x86
--enable-match-finders=hc4,bt2,bt4
--enable-checks=crc32,sha256
--enable-small
--enable-threads=yes
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-fast-install
--enable-unsafe-type-punning
--enable-static
--disable-assembler
--disable-lzma-links
--disable-silent-rules
--disable-libtool-lock
--disable-nls
--disable-largefile
