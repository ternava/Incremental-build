--enable-encoders=lzma1,delta
--enable-decoders=lzma1,ia64
--enable-match-finders=hc3,bt3
--enable-checks=crc32
--enable-threads=no
--enable-symbol-versions
--enable-sandbox=no
--enable-silent-rules
--enable-dependency-tracking
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-option-checking
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-silent-rules
--disable-dependency-tracking
--disable-nls
--disable-rpath
