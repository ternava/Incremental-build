--enable-debug
--enable-encoders=lzma1,delta
--enable-decoders=lzma1,x86
--enable-match-finders=hc3,bt2,bt3
--enable-small
--enable-threads=yes
--enable-assume-ram=128
--enable-sandbox=no
--enable-static
--enable-fast-install=no
--disable-xzdec
--disable-lzmainfo
--disable-libtool-lock
--disable-largefile
