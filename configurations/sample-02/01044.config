--enable-encoders=lzma1,arm
--enable-decoders=lzma1,armthumb
--enable-match-finders=bt4
--enable-checks=crc32,crc64
--enable-threads=posix
--enable-shared=no
--enable-sandbox=auto
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-dependency-tracking
--disable-nls
--disable-largefile
