--enable-debug
--enable-encoders=lzma1,arm
--enable-decoders=lzma1,delta
--enable-match-finders=hc4,bt2,bt3,bt4
--enable-checks=crc32,sha256
--enable-threads=yes
--enable-shared=no
--enable-sandbox=auto
--enable-dependency-tracking
--enable-static
--enable-fast-install=no
--disable-xzdec
--disable-libtool-lock
--disable-largefile
