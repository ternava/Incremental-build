--enable-encoders=lzma1,ia64
--enable-decoders=lzma1,delta
--enable-match-finders=bt4
--enable-checks=crc32,crc64,sha256
--enable-small
--enable-threads=yes
--enable-shared=no
--enable-sandbox=auto
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install
--disable-option-checking
--disable-xzdec
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-nls
