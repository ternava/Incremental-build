--enable-debug
--enable-encoders=lzma1,delta
--enable-decoders=lzma1,lzma2
--enable-match-finders=hc3,bt4
--enable-checks=crc32,sha256
--enable-threads=no
--enable-shared=no
--enable-symbol-versions
--enable-silent-rules
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-assembler
--disable-scripts
--disable-nls
--disable-rpath
