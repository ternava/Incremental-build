--enable-encoders=lzma1,arm
--enable-decoders=lzma1,x86
--enable-match-finders=bt3,bt4
--enable-checks=crc32,crc64
--enable-small
--enable-shared=no
--enable-symbol-versions
--enable-fast-install
--enable-static
