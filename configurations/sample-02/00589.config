--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1,ia64
--enable-match-finders=bt2,bt4
--enable-threads=no
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=no
--enable-unsafe-type-punning
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-lzma-links
--disable-nls
--disable-rpath
--disable-largefile
