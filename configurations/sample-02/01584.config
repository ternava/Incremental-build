--enable-debug
--enable-encoders=lzma1,delta
--enable-decoders=lzma1,sparc
--enable-match-finders=hc4,hc3,bt2,bt3,bt4
--enable-checks=crc32
--enable-threads=no
--enable-shared=no
--enable-sandbox=no
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--disable-option-checking
--disable-assembler
--disable-lzmainfo
--disable-scripts
--disable-doc
--disable-dependency-tracking
--disable-nls
--disable-rpath
