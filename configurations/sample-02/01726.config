--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1
--enable-match-finders=hc4,bt2,bt4
--enable-checks=crc32
--enable-small
--enable-threads=posix
--enable-assume-ram=128
--enable-sandbox=auto
--enable-dependency-tracking
