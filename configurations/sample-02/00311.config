--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,powerpc
--enable-match-finders=hc4,hc3,bt4
--enable-checks=crc32,crc64,sha256
--enable-threads=no
--enable-assume-ram=128
--enable-symbol-versions
--enable-sandbox=no
--enable-silent-rules
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-lzma-links
--disable-doc
--disable-dependency-tracking
--disable-nls
