--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,armthumb
--enable-match-finders=hc4,hc3,bt2,bt4
--enable-checks=crc32
--enable-threads=no
--enable-symbol-versions
--enable-sandbox=no
--enable-silent-rules
--enable-unaligned-access
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-lzmainfo
--disable-scripts
--disable-doc
--disable-nls
--disable-rpath
--disable-largefile
