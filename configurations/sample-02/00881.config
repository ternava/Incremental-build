--enable-debug
--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1,delta
--enable-match-finders=hc4,bt2
--enable-checks=crc32,crc64
--enable-threads=posix
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=no
--enable-silent-rules
--enable-fast-install
--enable-unaligned-access
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-scripts
--disable-doc
--disable-dependency-tracking
--disable-libtool-lock
