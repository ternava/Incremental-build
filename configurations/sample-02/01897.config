--enable-debug
--enable-encoders=lzma1,arm
--enable-decoders=lzma1,sparc
--enable-match-finders=hc3,hc4
--enable-checks=crc32
--enable-small
--enable-symbol-versions
--enable-sandbox=no
--enable-dependency-tracking
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-option-checking
--disable-lzmadec
--disable-lzmainfo
--disable-doc
--disable-dependency-tracking
--disable-nls
--disable-rpath
--disable-largefile
