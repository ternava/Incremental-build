--enable-debug
--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,ia64
--enable-match-finders=hc3,bt2,bt4
--enable-checks=crc32,crc64
--enable-threads=yes
--enable-shared=no
--enable-sandbox=no
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-xzdec
--disable-lzma-links
--disable-libtool-lock
--disable-rpath
