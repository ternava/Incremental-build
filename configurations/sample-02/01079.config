--enable-encoders=lzma1,x86
--enable-decoders=lzma1,ia64
--enable-match-finders=bt3,bt4
--enable-checks=crc32,sha256
--enable-threads=yes
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-silent-rules
--enable-fast-install
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-assembler
--disable-xzdec
--disable-lzmainfo
--disable-lzma-links
--disable-doc
--disable-libtool-lock
--disable-rpath
