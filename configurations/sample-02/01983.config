--enable-debug
--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,lzma2
--enable-match-finders=hc4,bt2
--enable-checks=crc32,crc64,sha256
--enable-small
--enable-threads=yes
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-doc
--disable-silent-rules
--disable-dependency-tracking
--disable-libtool-lock
--disable-largefile
