--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1
--enable-match-finders=hc4
--enable-checks=crc32,sha256
--enable-threads=yes
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=no
--enable-silent-rules
--enable-dependency-tracking
--enable-unsafe-type-punning
--enable-static
--disable-assembler
--disable-scripts
--disable-silent-rules
--disable-libtool-lock
--disable-rpath
