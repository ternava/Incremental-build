--enable-encoders=lzma1
--enable-decoders=lzma1,ia64
--enable-match-finders=hc4,bt4
--enable-checks=crc32
--enable-threads=no
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--disable-xzdec
--disable-lzmainfo
--disable-dependency-tracking
--disable-rpath
