--enable-debug
--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,armthumb
--enable-match-finders=hc3,bt2,bt3
--enable-checks=crc32,crc64,sha256
--enable-threads=no
--enable-shared=no
--enable-sandbox=no
--enable-silent-rules
--enable-fast-install
--enable-unaligned-access
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-lzma-links
--disable-dependency-tracking
--disable-libtool-lock
