--enable-debug
--enable-encoders=lzma1,ia64
--enable-decoders=lzma1,lzma2
--enable-match-finders=bt3
--enable-checks=crc32
--enable-threads=posix
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-dependency-tracking
--enable-fast-install
--enable-static
--disable-xzdec
--disable-lzmainfo
--disable-doc
--disable-libtool-lock
--disable-rpath
