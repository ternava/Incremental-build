--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,x86
--enable-match-finders=hc4
--enable-checks=crc32,sha256
--enable-threads=yes
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
