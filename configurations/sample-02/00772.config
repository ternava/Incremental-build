--enable-debug
--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,lzma2
--enable-match-finders=bt2,bt3
--enable-threads=yes
--enable-assume-ram=128
--enable-symbol-versions
--enable-silent-rules
--enable-fast-install
--enable-static
--disable-option-checking
--disable-assembler
--disable-lzma-links
--disable-doc
--disable-dependency-tracking
--disable-nls
--disable-rpath
