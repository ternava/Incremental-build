--enable-encoders=lzma1,arm
--enable-decoders=lzma1,powerpc
--enable-match-finders=hc3,bt3,bt4
--enable-checks=crc32,crc64
--enable-small
--enable-threads=yes
--enable-assume-ram=128
--enable-symbol-versions
--enable-silent-rules
--enable-dependency-tracking
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-libtool-lock
--disable-nls
--disable-largefile
