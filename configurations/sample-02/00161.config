--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1,sparc
--enable-match-finders=bt3
--enable-threads=yes
--enable-assume-ram=128
--enable-symbol-versions
--enable-silent-rules
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-doc
--disable-silent-rules
--disable-dependency-tracking
--disable-libtool-lock
