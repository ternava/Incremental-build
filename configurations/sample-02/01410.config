--enable-encoders=lzma1
--enable-decoders=lzma1,armthumb
--enable-match-finders=bt2
--enable-checks=crc32,crc64
--enable-small
--enable-threads=posix
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install
--enable-unaligned-access
--enable-static
--enable-fast-install=no
--disable-xzdec
--disable-lzma-links
--disable-doc
--disable-libtool-lock
--disable-largefile
