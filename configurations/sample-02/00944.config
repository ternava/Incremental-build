--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1,x86
--enable-match-finders=hc3,bt2,bt3,bt4
--enable-checks=crc32,crc64,sha256
--enable-threads=yes
--enable-assume-ram=128
--enable-sandbox=auto
--enable-dependency-tracking
--enable-fast-install
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-xzdec
--disable-lzmadec
--disable-lzma-links
--disable-scripts
--disable-silent-rules
--disable-libtool-lock
--disable-rpath
