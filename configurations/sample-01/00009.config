--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,lzma2
--enable-match-finders=bt2,bt3
--enable-checks=crc32
--enable-threads=no
--enable-assume-ram=128
--enable-symbol-versions
--enable-sandbox=auto
--enable-fast-install
--enable-unsafe-type-punning
--disable-assembler
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-nls
