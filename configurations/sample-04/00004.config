--enable-encoders=lzma1,x86
--enable-decoders=lzma1,sparc
--enable-match-finders=bt4
--enable-checks=crc32,crc64
--enable-threads=no
--enable-shared=no
--enable-sandbox=no
--enable-silent-rules
--enable-fast-install
--enable-unaligned-access
--enable-static
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-doc
