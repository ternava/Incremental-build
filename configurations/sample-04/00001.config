--enable-debug
--enable-encoders=lzma1,delta
--enable-decoders=lzma1,sparc
--enable-match-finders=hc3,bt2,bt4
--enable-checks=crc32,sha256
--enable-shared=no
--enable-sandbox=no
--enable-silent-rules
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-xzdec
--disable-lzmainfo
--disable-scripts
--disable-dependency-tracking
--disable-nls
