--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,ia64
--enable-match-finders=bt2,bt4
--enable-checks=crc32,sha256
--enable-threads=yes
--enable-sandbox=no
--enable-silent-rules
--enable-dependency-tracking
--enable-unaligned-access
--enable-static
