--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1,lzma2
--enable-match-finders=hc4,hc3,bt3
--enable-checks=crc32
--enable-threads=posix
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
