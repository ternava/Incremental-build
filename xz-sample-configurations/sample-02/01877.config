--enable-encoders=lzma1
--enable-decoders=lzma1,lzma2
--enable-match-finders=hc4,hc3,bt3
--enable-checks=crc32,crc64
--enable-threads=yes
--enable-assume-ram=128
--enable-sandbox=capsicum
--enable-silent-rules
--enable-dependency-tracking
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-lzmadec
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-largefile
