--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1,delta
--enable-match-finders=bt2
--enable-checks=crc32,sha256
--enable-threads=yes
--enable-shared=no
--enable-sandbox=capsicum
--enable-fast-install
--enable-unaligned-access
--enable-static
--disable-assembler
--disable-xzdec
--disable-silent-rules
--disable-dependency-tracking
--disable-libtool-lock
--disable-rpath
