--enable-debug
--enable-encoders=lzma1,ia64
--enable-decoders=lzma1,lzma2
--enable-match-finders=bt2
--enable-threads=no
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install
--enable-unaligned-access
--enable-fast-install=no
--disable-xzdec
--disable-lzmainfo
--disable-lzma-links
--disable-nls
--disable-largefile
