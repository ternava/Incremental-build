--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,lzma2
--enable-match-finders=hc3,bt2,bt4
--enable-checks=crc32
--enable-threads=no
--enable-symbol-versions
--enable-sandbox=no
--enable-dependency-tracking
--enable-unaligned-access
--enable-static
--enable-fast-install=no
--disable-xzdec
--disable-lzmainfo
--disable-scripts
--disable-doc
--disable-libtool-lock
--disable-nls
