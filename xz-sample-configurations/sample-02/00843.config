--enable-debug
--enable-encoders=lzma1,arm
--enable-decoders=lzma1,lzma2
--enable-match-finders=hc3,bt3,bt4
--enable-checks=crc32
--enable-small
--enable-threads=posix
--enable-shared=no
--enable-sandbox=capsicum
--enable-fast-install
--enable-unsafe-type-punning
--enable-static
--disable-option-checking
--disable-scripts
--disable-silent-rules
--disable-libtool-lock
--disable-rpath
--disable-largefile
