--enable-encoders=lzma1,delta
--enable-decoders=lzma1,powerpc
--enable-match-finders=hc4
--enable-checks=crc32,crc64
--enable-small
--enable-threads=posix
--enable-assume-ram=128
--enable-sandbox=no
--enable-dependency-tracking
--enable-fast-install
--disable-option-checking
--disable-assembler
--disable-lzmainfo
--disable-lzma-links
--disable-libtool-lock
--disable-largefile
