--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1
--enable-match-finders=hc3,bt3,bt4
--enable-checks=crc32
--enable-small
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-dependency-tracking
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-silent-rules
--disable-nls
--disable-rpath
--disable-largefile
