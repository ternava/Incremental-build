--enable-debug
--enable-encoders=lzma1,delta
--enable-decoders=lzma1,powerpc
--enable-match-finders=hc4,bt3
--enable-checks=crc32,crc64,sha256
--enable-threads=no
--enable-shared=no
--enable-sandbox=auto
--enable-dependency-tracking
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-lzmadec
--disable-lzma-links
--disable-scripts
--disable-nls
--disable-largefile
