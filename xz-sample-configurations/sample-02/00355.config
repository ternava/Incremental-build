--enable-debug
--enable-encoders=lzma1,lzma2,x86,armthumb,sparc,arm,ia64,powerpc,delta
--enable-decoders=lzma1,sparc
--enable-match-finders=bt4
--enable-checks=crc32
--enable-threads=no
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=no
--enable-fast-install
--enable-unsafe-type-punning
--enable-static
--disable-option-checking
--disable-lzmainfo
--disable-lzma-links
--disable-dependency-tracking
--disable-largefile
