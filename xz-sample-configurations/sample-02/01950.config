--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1,ia64
--enable-match-finders=bt4
--enable-checks=crc32
--enable-small
--enable-threads=posix
--enable-sandbox=no
--enable-fast-install
--enable-unsafe-type-punning
--enable-static
--disable-option-checking
--disable-xzdec
--disable-scripts
--disable-doc
--disable-libtool-lock
--disable-nls
