--enable-debug
--enable-encoders=lzma1,sparc
--enable-decoders=lzma1
--enable-match-finders=hc4,hc3,bt3,bt4
--enable-checks=crc32,crc64
--enable-small
--enable-threads=yes
--enable-shared=no
--enable-sandbox=capsicum
--enable-unsafe-type-punning
--disable-option-checking
--disable-lzmadec
--disable-lzmainfo
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
