--enable-encoders=lzma1,arm
--enable-decoders=lzma1,delta
--enable-match-finders=hc3,bt2
--enable-checks=crc32,crc64
--enable-threads=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-dependency-tracking
--enable-fast-install
--enable-unaligned-access
--disable-lzmadec
--disable-scripts
--disable-libtool-lock
--disable-nls
--disable-rpath
