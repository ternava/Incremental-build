--enable-encoders=lzma1
--enable-decoders=lzma1,arm
--enable-match-finders=hc4,bt2,bt4
--enable-threads=no
--enable-symbol-versions
--enable-silent-rules
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-doc
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
