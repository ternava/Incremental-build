--enable-debug
--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,ia64
--enable-match-finders=hc3,bt3
--enable-checks=crc32
--enable-small
--enable-threads=yes
--enable-assume-ram=128
--enable-shared=no
--enable-sandbox=auto
--enable-silent-rules
--enable-dependency-tracking
--enable-unsafe-type-punning
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzmainfo
--disable-libtool-lock
--disable-rpath
