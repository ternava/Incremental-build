--enable-encoders=lzma1,delta
--enable-decoders=lzma1,powerpc
--enable-match-finders=hc4,bt2,bt4
--enable-checks=crc32,sha256
--enable-threads=yes
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=no
--enable-silent-rules
--enable-fast-install
--enable-unaligned-access
--disable-option-checking
--disable-xzdec
--disable-lzma-links
--disable-dependency-tracking
--disable-libtool-lock
--disable-rpath
--disable-largefile
