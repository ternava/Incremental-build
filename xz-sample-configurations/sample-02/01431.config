--enable-encoders=lzma1
--enable-decoders=lzma1,delta
--enable-match-finders=hc4,bt3,bt4
--enable-checks=crc32
--enable-assume-ram=128
--enable-symbol-versions
--enable-dependency-tracking
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-lzmainfo
--disable-lzma-links
--disable-doc
--disable-dependency-tracking
--disable-nls
--disable-rpath
--disable-largefile
