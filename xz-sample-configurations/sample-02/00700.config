--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1
--enable-match-finders=hc4
--enable-checks=crc32
--enable-threads=yes
--enable-assume-ram=128
--enable-shared=no
--enable-sandbox=auto
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-silent-rules
--disable-libtool-lock
--disable-nls
--disable-rpath
