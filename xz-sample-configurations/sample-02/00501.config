--enable-debug
--enable-encoders=lzma1,delta
--enable-decoders=lzma1,armthumb
--enable-match-finders=bt2,bt4
--enable-checks=crc32,crc64,sha256
--enable-threads=posix
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install
--enable-unsafe-type-punning
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-dependency-tracking
--disable-nls
--disable-rpath
--disable-largefile
