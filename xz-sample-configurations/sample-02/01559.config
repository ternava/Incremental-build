--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1
--enable-match-finders=hc3,bt3
--enable-checks=crc32,crc64
--enable-threads=no
--enable-sandbox=no
--enable-silent-rules
--enable-dependency-tracking
--enable-unaligned-access
--enable-static
--disable-option-checking
--disable-assembler
--disable-lzmainfo
--disable-lzma-links
--disable-silent-rules
--disable-nls
--disable-rpath
