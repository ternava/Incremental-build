--enable-debug
--enable-encoders=lzma1,ia64
--enable-decoders=lzma1,arm
--enable-match-finders=bt2
--enable-checks=crc32,crc64,sha256
--enable-small
--enable-threads=posix
--enable-symbol-versions
--enable-silent-rules
--enable-fast-install
--enable-unsafe-type-punning
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-silent-rules
--disable-dependency-tracking
--disable-libtool-lock
