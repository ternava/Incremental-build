--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1,x86
--enable-match-finders=hc4,bt4
--enable-checks=crc32,crc64
--enable-threads=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-xzdec
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-largefile
