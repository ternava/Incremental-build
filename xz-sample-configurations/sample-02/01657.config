--enable-debug
--enable-encoders=lzma1
--enable-decoders=lzma1,x86
--enable-match-finders=hc4,hc3,bt2
--enable-checks=crc32
--enable-threads=posix
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-unaligned-access
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-lzmainfo
--disable-lzma-links
--disable-doc
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
