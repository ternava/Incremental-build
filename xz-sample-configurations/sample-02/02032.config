--enable-debug
--enable-encoders=lzma1,arm
--enable-decoders=lzma1,armthumb
--enable-match-finders=bt4
--enable-checks=crc32,sha256
--enable-small
--enable-threads=posix
--enable-shared=no
--enable-sandbox=no
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--disable-assembler
--disable-lzmadec
--disable-lzma-links
--disable-scripts
--disable-libtool-lock
--disable-nls
--disable-rpath
