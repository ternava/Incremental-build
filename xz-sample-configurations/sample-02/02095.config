--enable-encoders=lzma1,arm
--enable-decoders=lzma1,ia64
--enable-match-finders=hc4,bt2,bt4
--enable-checks=crc32
--enable-threads=no
--enable-silent-rules
--enable-fast-install
--enable-unaligned-access
--enable-static
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-lzma-links
--disable-silent-rules
--disable-dependency-tracking
--disable-libtool-lock
--disable-rpath
