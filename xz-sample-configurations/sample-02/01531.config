--enable-debug
--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1,sparc
--enable-match-finders=bt3
--enable-checks=crc32,sha256
--enable-threads=yes
--enable-assume-ram=128
--enable-sandbox=capsicum
--enable-dependency-tracking
--disable-option-checking
--disable-xzdec
--disable-lzmainfo
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-rpath
