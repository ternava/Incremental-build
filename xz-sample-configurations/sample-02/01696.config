--enable-debug
--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1,x86
--enable-match-finders=bt3
--enable-checks=crc32,crc64
--enable-small
--enable-threads=yes
--enable-assume-ram=128
--enable-sandbox=capsicum
--enable-silent-rules
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-lzmadec
--disable-lzmainfo
--disable-silent-rules
--disable-rpath
