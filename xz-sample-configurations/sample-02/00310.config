--enable-encoders=lzma1,arm
--enable-decoders=lzma1,x86
--enable-match-finders=hc4,hc3,bt2,bt3,bt4
--enable-checks=crc32
--enable-threads=no
--enable-symbol-versions
--enable-dependency-tracking
--enable-unaligned-access
--disable-option-checking
--disable-xzdec
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-rpath
