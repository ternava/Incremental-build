--enable-encoders=lzma1
--enable-decoders=lzma1,arm
--enable-match-finders=hc4,bt3,bt4
--enable-checks=crc32,crc64
--enable-threads=posix
--enable-sandbox=auto
--enable-fast-install
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-lzmadec
--disable-lzma-links
--disable-doc
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
