--enable-encoders=lzma1,arm
--enable-decoders=lzma1,lzma2
--enable-match-finders=hc4
--enable-checks=crc32,sha256
--enable-small
--enable-threads=yes
--enable-silent-rules
--enable-fast-install
--enable-unaligned-access
--enable-static
--disable-option-checking
--disable-lzmadec
--disable-lzmainfo
--disable-doc
--disable-dependency-tracking
--disable-nls
--disable-largefile
