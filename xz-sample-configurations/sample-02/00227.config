--enable-debug
--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,lzma2
--enable-match-finders=hc3,bt4
--enable-checks=crc32
--enable-small
--enable-threads=yes
--enable-assume-ram=128
--enable-shared=no
--enable-sandbox=auto
--enable-unaligned-access
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-lzmainfo
--disable-lzma-links
--disable-silent-rules
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
