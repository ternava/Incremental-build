--enable-encoders=lzma1
--enable-decoders=lzma1,lzma2
--enable-match-finders=hc3,bt2,bt4
--enable-threads=yes
--enable-shared=no
--enable-symbol-versions
--enable-silent-rules
--enable-fast-install
--enable-unaligned-access
--enable-static
--disable-assembler
--disable-lzmainfo
--disable-libtool-lock
--disable-nls
--disable-rpath
