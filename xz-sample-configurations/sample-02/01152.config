--enable-encoders=lzma1,delta
--enable-decoders=lzma1,arm
--enable-match-finders=hc4,hc3,bt3
--enable-checks=crc32,sha256
--enable-threads=no
--enable-sandbox=no
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-xzdec
--disable-lzma-links
--disable-doc
--disable-libtool-lock
--disable-nls
