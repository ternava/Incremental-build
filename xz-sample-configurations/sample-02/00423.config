--enable-debug
--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1,delta
--enable-match-finders=hc4,bt2,bt4
--enable-checks=crc32,sha256
--enable-threads=yes
--enable-shared=no
--enable-sandbox=auto
--enable-silent-rules
--enable-unsafe-type-punning
--enable-static
--disable-option-checking
--disable-lzmainfo
--disable-lzma-links
--disable-silent-rules
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
