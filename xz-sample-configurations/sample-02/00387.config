--enable-encoders=lzma1,x86
--enable-decoders=lzma1,lzma2,delta,x86,sparc,ia64,armthumb,arm,powerpc
--enable-match-finders=bt2,bt4
--enable-checks=crc32
--enable-threads=yes
--enable-shared=no
--enable-sandbox=auto
--enable-fast-install
--enable-unaligned-access
--enable-static
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-doc
--disable-silent-rules
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
