--enable-encoders=lzma1,arm
--enable-decoders=lzma1,delta
--enable-match-finders=hc4,bt3
--enable-checks=crc32,crc64
--enable-small
--enable-threads=posix
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=no
--enable-silent-rules
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-lzmainfo
--disable-scripts
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
