--enable-encoders=lzma1,delta
--enable-decoders=lzma1,arm
--enable-match-finders=hc4,bt3
--enable-checks=crc32
--enable-small
--enable-threads=posix
--enable-sandbox=capsicum
--enable-silent-rules
--enable-dependency-tracking
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-xzdec
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-nls
