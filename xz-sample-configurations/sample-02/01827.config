--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1,sparc
--enable-match-finders=hc3,bt2,bt3,bt4
--enable-checks=crc32
--enable-threads=no
--enable-shared=no
--enable-sandbox=capsicum
--enable-dependency-tracking
--enable-fast-install
--enable-unaligned-access
--disable-xzdec
--disable-lzmainfo
--disable-lzma-links
--disable-silent-rules
--disable-nls
--disable-largefile
