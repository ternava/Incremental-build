--enable-debug
--enable-encoders=lzma1
--enable-decoders=lzma1,x86
--enable-match-finders=hc3,hc4
--enable-checks=crc32
--enable-small
--enable-threads=posix
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-dependency-tracking
--disable-assembler
--disable-lzmadec
--disable-lzmainfo
--disable-scripts
--disable-silent-rules
--disable-libtool-lock
