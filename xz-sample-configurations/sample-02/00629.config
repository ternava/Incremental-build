--enable-debug
--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1,arm
--enable-match-finders=bt2,bt3
--enable-checks=crc32
--enable-small
--enable-threads=yes
--enable-symbol-versions
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install
--enable-unaligned-access
--enable-static
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-largefile
