--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1,powerpc
--enable-match-finders=bt2,bt4
--enable-checks=crc32,crc64
--enable-threads=yes
--enable-assume-ram=128
--enable-sandbox=no
--enable-silent-rules
--enable-fast-install
--enable-unaligned-access
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-lzmainfo
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
