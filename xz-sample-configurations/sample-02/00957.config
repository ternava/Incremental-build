--enable-encoders=lzma1,sparc
--enable-decoders=lzma1
--enable-match-finders=hc3,bt2,bt3,bt4
--enable-checks=crc32,sha256
--enable-threads=yes
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-lzmainfo
--disable-scripts
--disable-doc
--disable-nls
--disable-rpath
--disable-largefile
