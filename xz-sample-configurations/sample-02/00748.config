--enable-debug
--enable-encoders=lzma1,delta
--enable-decoders=lzma1,lzma2
--enable-match-finders=bt3,bt4
--enable-checks=crc32,crc64
--enable-threads=posix
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=no
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--disable-option-checking
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
