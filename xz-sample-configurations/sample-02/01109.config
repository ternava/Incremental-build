--enable-debug
--enable-encoders=lzma1
--enable-decoders=lzma1,armthumb
--enable-match-finders=hc4,bt3,bt4
--enable-checks=crc32
--enable-threads=posix
--enable-sandbox=auto
--enable-fast-install
--disable-option-checking
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-scripts
--disable-dependency-tracking
--disable-libtool-lock
--disable-rpath
