--enable-debug
--enable-encoders=lzma1,lzma2,x86,armthumb,sparc,arm,ia64,powerpc,delta
--enable-decoders=lzma1,delta
--enable-match-finders=hc4,bt4
--enable-threads=yes
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=no
--enable-dependency-tracking
--enable-unaligned-access
--enable-static
--disable-lzmadec
--disable-lzmainfo
--disable-doc
--disable-silent-rules
--disable-libtool-lock
