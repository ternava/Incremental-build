--enable-debug
--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1,x86
--enable-match-finders=hc3
--enable-checks=crc32
--enable-small
--enable-shared=no
--enable-sandbox=capsicum
--enable-unaligned-access
--enable-fast-install=no
--disable-assembler
--disable-lzmadec
--disable-lzma-links
--disable-scripts
--disable-silent-rules
