--enable-debug
--enable-encoders=lzma1,lzma2,x86,armthumb,sparc,arm,ia64,powerpc,delta
--enable-decoders=lzma1,arm
--enable-match-finders=hc4,hc3,bt4
--enable-checks=crc32
--enable-threads=posix
--enable-sandbox=capsicum
--enable-unaligned-access
--enable-static
--enable-fast-install=no
--disable-xzdec
--disable-lzma-links
--disable-scripts
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-largefile
