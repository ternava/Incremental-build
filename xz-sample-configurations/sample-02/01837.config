--enable-debug
--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,sparc
--enable-match-finders=hc4
--enable-checks=crc32
--enable-threads=posix
--enable-assume-ram=128
--enable-sandbox=auto
--enable-fast-install
--enable-unsafe-type-punning
