--enable-encoders=lzma1,delta
--enable-decoders=lzma1,arm
--enable-match-finders=hc4,bt2
--enable-checks=crc32,sha256
--enable-threads=posix
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-unaligned-access
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-scripts
--disable-doc
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
