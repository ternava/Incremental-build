--enable-encoders=lzma1,ia64
--enable-decoders=lzma1,x86
--enable-match-finders=bt4
--enable-checks=crc32,crc64
--enable-small
--enable-threads=posix
--enable-symbol-versions
--enable-sandbox=no
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-silent-rules
--disable-dependency-tracking
--disable-libtool-lock
--disable-largefile
