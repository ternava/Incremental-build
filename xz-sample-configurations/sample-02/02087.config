--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1,sparc
--enable-match-finders=hc4,hc3,bt3,bt4
--enable-small
--enable-threads=yes
--enable-shared=no
--enable-sandbox=auto
--enable-dependency-tracking
--enable-unaligned-access
--enable-unsafe-type-punning
--disable-option-checking
--disable-xzdec
--disable-lzma-links
--disable-silent-rules
--disable-dependency-tracking
--disable-libtool-lock
