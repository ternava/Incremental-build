--enable-debug
--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,lzma2,delta,x86,sparc,ia64,armthumb,arm,powerpc
--enable-match-finders=hc4,hc3,bt2,bt3,bt4
--enable-checks=crc32,sha256
--enable-small
--enable-threads=posix
--enable-shared=no
--enable-sandbox=no
--enable-dependency-tracking
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-lzmainfo
--disable-lzma-links
--disable-dependency-tracking
--disable-libtool-lock
