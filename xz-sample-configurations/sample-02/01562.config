--enable-debug
--enable-encoders=lzma1
--enable-decoders=lzma1,armthumb
--enable-match-finders=hc3,bt4
--enable-checks=crc32,sha256
--enable-small
--enable-threads=yes
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-lzma-links
--disable-doc
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
