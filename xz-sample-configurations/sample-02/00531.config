--enable-encoders=lzma1,x86
--enable-decoders=lzma1,sparc
--enable-match-finders=bt2,bt3,bt4
--enable-checks=crc32,crc64,sha256
--enable-threads=posix
--enable-silent-rules
--enable-dependency-tracking
--enable-unaligned-access
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-xzdec
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-dependency-tracking
--disable-nls
