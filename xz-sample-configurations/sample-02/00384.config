--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1
--enable-match-finders=hc4,bt2,bt3,bt4
--enable-checks=crc32
--enable-small
--enable-threads=yes
--enable-shared=no
--enable-sandbox=capsicum
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-option-checking
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-libtool-lock
--disable-rpath
--disable-largefile
