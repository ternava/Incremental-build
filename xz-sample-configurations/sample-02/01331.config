--enable-debug
--enable-encoders=lzma1,delta
--enable-decoders=lzma1,ia64
--enable-match-finders=hc3,bt4
--enable-checks=crc32,sha256
--enable-small
--enable-threads=posix
--enable-assume-ram=128
--enable-shared=no
--enable-sandbox=capsicum
--enable-dependency-tracking
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--disable-xzdec
--disable-lzma-links
--disable-doc
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
