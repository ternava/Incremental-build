--enable-debug
--enable-encoders=lzma1
--enable-decoders=lzma1,lzma2,delta,x86,sparc,ia64,armthumb,arm,powerpc
--enable-match-finders=hc4,bt4
--enable-checks=crc32
--enable-threads=no
--enable-assume-ram=128
--enable-symbol-versions
--enable-dependency-tracking
--enable-fast-install
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-lzma-links
--disable-scripts
--disable-silent-rules
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
