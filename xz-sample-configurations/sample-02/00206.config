--enable-encoders=lzma1,delta
--enable-decoders=lzma1,x86
--enable-match-finders=hc4,bt2,bt4
--enable-checks=crc32,sha256
--enable-small
--enable-threads=posix
--enable-assume-ram=128
--enable-sandbox=capsicum
--enable-silent-rules
--enable-fast-install
--enable-unaligned-access
--enable-static
--disable-assembler
--disable-xzdec
--disable-lzmainfo
--disable-lzma-links
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
