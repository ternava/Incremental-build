--enable-encoders=lzma1,delta
--enable-decoders=lzma1,lzma2
--enable-match-finders=hc4,bt3
--enable-checks=crc32,sha256
--enable-small
--enable-assume-ram=128
--enable-shared=no
--enable-sandbox=no
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzmainfo
--disable-doc
--disable-dependency-tracking
--disable-nls
--disable-rpath
