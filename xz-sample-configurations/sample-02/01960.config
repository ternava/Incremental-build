--enable-debug
--enable-encoders=lzma1,arm
--enable-decoders=lzma1,x86
--enable-match-finders=hc3,bt2
--enable-checks=crc32,sha256
--enable-small
--enable-threads=yes
--enable-assume-ram=128
--enable-shared=no
--enable-sandbox=no
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--disable-xzdec
--disable-libtool-lock
--disable-rpath
--disable-largefile
