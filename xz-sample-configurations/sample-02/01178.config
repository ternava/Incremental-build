--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,lzma2
--enable-match-finders=hc3
--enable-checks=crc32,crc64
--enable-small
--enable-threads=posix
--enable-sandbox=auto
--enable-unaligned-access
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-lzmainfo
--disable-lzma-links
--disable-doc
--disable-largefile
