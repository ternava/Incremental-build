--enable-debug
--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1,ia64
--enable-match-finders=hc4,bt2
--enable-checks=crc32,sha256
--enable-threads=no
--enable-assume-ram=128
--enable-symbol-versions
--enable-sandbox=auto
--enable-dependency-tracking
--enable-fast-install
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-lzma-links
--disable-scripts
--disable-libtool-lock
--disable-largefile
