--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1,sparc
--enable-match-finders=bt2,bt3,bt4
--enable-checks=crc32,sha256
--enable-threads=posix
--enable-sandbox=no
--enable-dependency-tracking
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-assembler
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-nls
--disable-rpath
--disable-largefile
