--enable-debug
--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,arm
--enable-match-finders=bt2,bt4
--enable-checks=crc32,crc64
--enable-small
--enable-shared=no
--enable-sandbox=auto
--enable-silent-rules
--enable-dependency-tracking
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-assembler
--disable-xzdec
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-dependency-tracking
--disable-nls
--disable-largefile
