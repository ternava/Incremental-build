--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1,powerpc
--enable-match-finders=bt2,bt3,bt4
--enable-checks=crc32
--enable-threads=yes
--enable-shared=no
--enable-sandbox=auto
--enable-silent-rules
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-doc
--disable-silent-rules
--disable-nls
--disable-rpath
--disable-largefile
