--enable-debug
--enable-encoders=lzma1,ia64
--enable-decoders=lzma1,ia64
--enable-match-finders=bt2
--enable-checks=crc32,crc64
--enable-threads=yes
--enable-assume-ram=128
--enable-shared=no
--enable-sandbox=capsicum
--enable-silent-rules
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-dependency-tracking
--disable-rpath
--disable-largefile
