--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1,ia64
--enable-match-finders=hc4,bt3
--enable-checks=crc32,sha256
--enable-small
--enable-assume-ram=128
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install
--enable-unaligned-access
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-lzmainfo
--disable-doc
--disable-libtool-lock
--disable-rpath
