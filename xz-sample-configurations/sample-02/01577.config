--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,armthumb
--enable-match-finders=bt2,bt3,bt4
--enable-checks=crc32,sha256
--enable-threads=yes
--enable-sandbox=auto
--enable-dependency-tracking
--enable-fast-install
--enable-unsafe-type-punning
--enable-static
--disable-xzdec
--disable-lzmainfo
--disable-silent-rules
