--enable-debug
--enable-encoders=lzma1,delta
--enable-decoders=lzma1,arm
--enable-match-finders=hc3,bt4
--enable-checks=crc32
--enable-threads=yes
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-unaligned-access
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-silent-rules
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
