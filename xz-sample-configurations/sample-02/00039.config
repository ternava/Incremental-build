--enable-debug
--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,lzma2,delta,x86,sparc,ia64,armthumb,arm,powerpc
--enable-match-finders=hc4,hc3,bt2,bt3
--enable-checks=crc32
--enable-small
--enable-threads=yes
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-fast-install
--enable-static
--disable-assembler
--disable-lzmadec
--disable-lzmainfo
--disable-silent-rules
--disable-libtool-lock
--disable-rpath
