--enable-debug
--enable-encoders=lzma1,ia64
--enable-decoders=lzma1,arm
--enable-match-finders=hc3,bt3
--enable-checks=crc32,crc64
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-option-checking
--disable-xzdec
--disable-lzmainfo
--disable-lzma-links
--disable-silent-rules
--disable-nls
--disable-rpath
--disable-largefile
