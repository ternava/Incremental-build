--enable-debug
--enable-encoders=lzma1,sparc
--enable-decoders=lzma1
--enable-match-finders=bt3,bt4
--enable-checks=crc32,sha256
--enable-threads=no
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=no
--enable-silent-rules
--enable-fast-install
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-xzdec
--disable-lzma-links
--disable-silent-rules
--disable-libtool-lock
--disable-rpath
--disable-largefile
