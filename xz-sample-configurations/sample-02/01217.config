--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1,ia64
--enable-match-finders=bt2,bt3,bt4
--enable-checks=crc32,crc64
--enable-threads=no
--enable-shared=no
--enable-silent-rules
--enable-dependency-tracking
--enable-static
--disable-option-checking
--disable-lzmadec
--disable-silent-rules
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-rpath
