--enable-encoders=lzma1,delta
--enable-decoders=lzma1,ia64
--enable-match-finders=hc4,hc3,bt3
--enable-checks=crc32,crc64
--enable-threads=yes
--enable-symbol-versions
--enable-fast-install
--enable-unsafe-type-punning
--enable-static
--disable-assembler
--disable-xzdec
--disable-lzmainfo
--disable-doc
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
