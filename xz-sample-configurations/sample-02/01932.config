--enable-encoders=lzma1,lzma2,x86,armthumb,sparc,arm,ia64,powerpc,delta
--enable-decoders=lzma1,ia64
--enable-match-finders=hc3,bt2,bt4
--enable-checks=crc32,crc64,sha256
--enable-threads=yes
--enable-sandbox=capsicum
--enable-dependency-tracking
--enable-unsafe-type-punning
--disable-option-checking
--disable-assembler
--disable-lzma-links
--disable-doc
--disable-largefile
