--enable-encoders=lzma1,arm
--enable-decoders=lzma1,x86
--enable-match-finders=bt2,bt3,bt4
--enable-checks=crc32,crc64
--enable-threads=no
--enable-shared=no
--enable-sandbox=no
--enable-silent-rules
--enable-dependency-tracking
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-doc
--disable-rpath
--disable-largefile
