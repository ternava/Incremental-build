--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,powerpc
--enable-match-finders=bt2,bt3
--enable-checks=crc32,crc64,sha256
--enable-threads=yes
--enable-assume-ram=128
--enable-sandbox=no
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-lzma-links
--disable-silent-rules
--disable-dependency-tracking
--disable-nls
--disable-rpath
