--enable-debug
--enable-encoders=lzma1,delta
--enable-decoders=lzma1,sparc
--enable-match-finders=bt4
--enable-checks=crc32,sha256
--enable-threads=yes
--enable-assume-ram=128
--enable-sandbox=auto
--enable-silent-rules
--enable-dependency-tracking
--enable-unaligned-access
--enable-static
--disable-option-checking
--disable-lzmadec
--disable-lzmainfo
--disable-doc
--disable-libtool-lock
--disable-nls
