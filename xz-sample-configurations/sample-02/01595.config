--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1,armthumb
--enable-match-finders=bt2,bt3,bt4
--enable-checks=crc32,crc64,sha256
--enable-threads=yes
--enable-sandbox=no
--enable-silent-rules
--enable-static
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-dependency-tracking
--disable-nls
--disable-rpath
