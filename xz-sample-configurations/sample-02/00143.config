--enable-debug
--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1
--enable-match-finders=bt2,bt3
--enable-checks=crc32,sha256
--enable-small
--enable-threads=yes
--enable-assume-ram=128
--enable-sandbox=auto
--enable-silent-rules
--enable-unsafe-type-punning
--enable-static
--disable-option-checking
--disable-xzdec
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-dependency-tracking
--disable-largefile
