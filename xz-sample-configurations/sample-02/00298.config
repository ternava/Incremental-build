--enable-encoders=lzma1,arm
--enable-decoders=lzma1,lzma2,delta,x86,sparc,ia64,armthumb,arm,powerpc
--enable-match-finders=bt2
--enable-checks=crc32,sha256
--enable-threads=posix
--enable-shared=no
--enable-sandbox=auto
--enable-unaligned-access
--disable-assembler
--disable-xzdec
--disable-lzma-links
--disable-scripts
--disable-silent-rules
--disable-nls
--disable-largefile
