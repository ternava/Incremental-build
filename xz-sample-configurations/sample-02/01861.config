--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1
--enable-match-finders=hc3,bt4
--enable-checks=crc32,crc64,sha256
--enable-small
--enable-threads=posix
--enable-sandbox=auto
--enable-silent-rules
--enable-dependency-tracking
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-option-checking
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-largefile
