--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,lzma2
--enable-match-finders=hc3,bt2,bt3
--enable-checks=crc32,crc64,sha256
--enable-small
--enable-threads=posix
--enable-shared=no
--enable-sandbox=no
--enable-silent-rules
--enable-unaligned-access
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-scripts
--disable-dependency-tracking
--disable-libtool-lock
--disable-rpath
--disable-largefile
