--enable-encoders=lzma1,delta
--enable-decoders=lzma1,ia64
--enable-match-finders=hc4,hc3,bt3
--enable-checks=crc32,crc64,sha256
--enable-small
--enable-sandbox=auto
--enable-silent-rules
--enable-unaligned-access
--enable-unsafe-type-punning
--disable-option-checking
--disable-assembler
--disable-silent-rules
--disable-libtool-lock
--disable-rpath
