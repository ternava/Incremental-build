--enable-encoders=lzma1,delta
--enable-decoders=lzma1,powerpc
--enable-match-finders=hc4,bt3,bt4
--enable-checks=crc32,sha256
--enable-threads=no
--enable-assume-ram=128
--enable-symbol-versions
--enable-silent-rules
--enable-dependency-tracking
--enable-unsafe-type-punning
--enable-static
--disable-xzdec
--disable-lzmainfo
--disable-doc
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
