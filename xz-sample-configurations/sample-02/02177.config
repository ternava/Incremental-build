--enable-debug
--enable-encoders=lzma1,sparc
--enable-decoders=lzma1
--enable-match-finders=hc4,bt2,bt3
--enable-checks=crc32,crc64
--enable-small
--enable-threads=posix
--enable-sandbox=capsicum
--enable-silent-rules
--enable-fast-install
--enable-unaligned-access
--enable-static
--enable-fast-install=no
--disable-xzdec
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
