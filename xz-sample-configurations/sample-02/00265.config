--enable-debug
--enable-encoders=lzma1
--enable-decoders=lzma1,delta
--enable-match-finders=hc4,bt4
--enable-checks=crc32,crc64
--enable-small
--enable-threads=posix
--enable-shared=no
--enable-sandbox=auto
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-lzmadec
--disable-lzma-links
--disable-scripts
--disable-dependency-tracking
--disable-nls
