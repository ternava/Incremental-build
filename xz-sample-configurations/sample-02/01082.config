--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1,delta
--enable-match-finders=hc3,bt2,bt3
--enable-checks=crc32,crc64
--enable-small
--enable-threads=yes
--enable-shared=no
--enable-silent-rules
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-lzmadec
--disable-scripts
--disable-dependency-tracking
--disable-nls
