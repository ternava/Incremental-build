--enable-debug
--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,lzma2,delta,x86,sparc,ia64,armthumb,arm,powerpc
--enable-match-finders=hc4,hc3,bt2
--enable-checks=crc32,crc64
--enable-small
--enable-threads=posix
--enable-symbol-versions
--enable-sandbox=auto
--enable-dependency-tracking
--enable-static
--disable-option-checking
--disable-assembler
--disable-lzmainfo
--disable-lzma-links
--disable-doc
--disable-silent-rules
--disable-rpath
--disable-largefile
