--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,delta
--enable-match-finders=hc4,hc3,bt4
--enable-checks=crc32,sha256
--enable-threads=yes
--enable-assume-ram=128
--enable-shared=no
--enable-sandbox=no
--enable-dependency-tracking
--enable-unsafe-type-punning
--disable-xzdec
--disable-lzmainfo
--disable-scripts
--disable-rpath
--disable-largefile
