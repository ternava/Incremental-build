--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,powerpc
--enable-match-finders=bt2
--enable-checks=crc32,crc64
--enable-small
--enable-threads=yes
--enable-assume-ram=128
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-fast-install=no
--disable-lzmadec
--disable-lzma-links
--disable-scripts
--disable-libtool-lock
--disable-nls
