--enable-encoders=lzma1,x86
--enable-decoders=lzma1,lzma2
--enable-match-finders=hc4,hc3,bt2,bt3,bt4
--enable-checks=crc32,sha256
--enable-threads=no
--enable-sandbox=auto
--enable-dependency-tracking
--disable-option-checking
--disable-xzdec
--disable-lzma-links
--disable-doc
--disable-libtool-lock
--disable-rpath
