--enable-debug
--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1,armthumb
--enable-match-finders=bt2,bt3
--enable-checks=crc32,crc64
--enable-threads=no
--enable-sandbox=auto
--enable-silent-rules
--enable-fast-install
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-doc
--disable-silent-rules
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-rpath
