--enable-debug
--enable-encoders=lzma1
--enable-decoders=lzma1,lzma2,delta,x86,sparc,ia64,armthumb,arm,powerpc
--enable-match-finders=hc4,bt2,bt3
--enable-threads=no
--enable-shared=no
--enable-sandbox=auto
--enable-fast-install
--enable-unaligned-access
--disable-option-checking
--disable-assembler
--disable-lzmainfo
--disable-scripts
--disable-doc
--disable-silent-rules
--disable-dependency-tracking
--disable-nls
--disable-rpath
--disable-largefile
