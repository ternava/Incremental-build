--enable-debug
--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1,armthumb
--enable-match-finders=bt4
--enable-checks=crc32,crc64,sha256
--enable-small
--enable-threads=posix
--enable-shared=no
--enable-symbol-versions
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--disable-assembler
--disable-lzmadec
--disable-scripts
--disable-libtool-lock
--disable-nls
--disable-rpath
