--enable-debug
--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1,ia64
--enable-match-finders=hc4,hc3,bt2,bt3,bt4
--enable-checks=crc32,sha256
--enable-threads=no
--enable-shared=no
--enable-symbol-versions
