--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1,sparc
--enable-match-finders=bt2
--enable-checks=crc32,crc64,sha256
--enable-small
--enable-threads=posix
--enable-sandbox=auto
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzma-links
--disable-libtool-lock
--disable-rpath
