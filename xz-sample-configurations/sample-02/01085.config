--enable-encoders=lzma1,arm
--enable-decoders=lzma1,armthumb
--enable-match-finders=hc4,bt3,bt4
--enable-checks=crc32,sha256
--enable-threads=yes
--enable-assume-ram=128
--enable-symbol-versions
--enable-silent-rules
--enable-fast-install
--enable-unaligned-access
--enable-static
--disable-assembler
--disable-lzmainfo
--disable-lzma-links
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-rpath
