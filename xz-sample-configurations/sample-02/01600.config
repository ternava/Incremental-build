--enable-encoders=lzma1,delta
--enable-decoders=lzma1,arm
--enable-match-finders=hc4,bt3,bt4
--enable-checks=crc32,sha256
--enable-threads=posix
--enable-sandbox=no
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install
--enable-static
--disable-option-checking
--disable-assembler
--disable-lzmainfo
--disable-scripts
--disable-doc
--disable-rpath
