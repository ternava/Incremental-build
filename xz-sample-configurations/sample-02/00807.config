--enable-debug
--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1,lzma2,delta,x86,sparc,ia64,armthumb,arm,powerpc
--enable-match-finders=hc4,hc3,bt3,bt4
--enable-checks=crc32,sha256
--enable-threads=no
--enable-sandbox=no
--enable-fast-install
--enable-static
--disable-option-checking
--disable-xzdec
--disable-lzma-links
--disable-doc
--disable-dependency-tracking
--disable-nls
--disable-largefile
