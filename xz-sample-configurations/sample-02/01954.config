--enable-encoders=lzma1,ia64
--enable-decoders=lzma1,delta
--enable-match-finders=hc4
--enable-checks=crc32,sha256
--enable-threads=posix
--enable-shared=no
--enable-sandbox=auto
--enable-dependency-tracking
--enable-unaligned-access
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
