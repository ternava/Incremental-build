--enable-debug
--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,armthumb
--enable-match-finders=hc4,bt2
--enable-checks=crc32,sha256
--enable-small
--enable-threads=yes
--enable-assume-ram=128
--enable-sandbox=no
--enable-silent-rules
--enable-unaligned-access
--enable-fast-install=no
--disable-option-checking
--disable-lzmadec
--disable-scripts
--disable-libtool-lock
--disable-nls
--disable-largefile
