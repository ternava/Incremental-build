--enable-encoders=lzma1
--enable-decoders=lzma1,sparc
--enable-match-finders=bt3
--enable-checks=crc32,crc64
--enable-threads=no
--enable-sandbox=capsicum
--enable-dependency-tracking
--enable-unsafe-type-punning
--enable-static
--disable-option-checking
--disable-xzdec
--disable-lzmadec
--disable-doc
--disable-silent-rules
--disable-libtool-lock
