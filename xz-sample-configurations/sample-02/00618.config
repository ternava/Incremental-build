--enable-encoders=lzma1,delta
--enable-decoders=lzma1,arm
--enable-match-finders=hc4,bt2
--enable-checks=crc32
--enable-small
--enable-threads=yes
--enable-assume-ram=128
--enable-shared=no
--enable-sandbox=capsicum
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-xzdec
--disable-scripts
--disable-doc
--disable-dependency-tracking
--disable-nls
--disable-largefile
