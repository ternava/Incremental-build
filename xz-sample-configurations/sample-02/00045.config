--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1,lzma2
--enable-match-finders=hc3,hc4
--enable-checks=crc32,sha256
--enable-small
--enable-threads=posix
--enable-assume-ram=128
--enable-silent-rules
--enable-unaligned-access
--enable-static
--disable-assembler
--disable-xzdec
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-libtool-lock
--disable-largefile
