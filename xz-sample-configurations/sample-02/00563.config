--enable-encoders=lzma1,lzma2,x86,armthumb,sparc,arm,ia64,powerpc,delta
--enable-decoders=lzma1,armthumb
--enable-match-finders=hc4,bt2,bt3,bt4
--enable-checks=crc32,crc64,sha256
--enable-threads=posix
--enable-assume-ram=128
--enable-symbol-versions
--enable-silent-rules
--enable-dependency-tracking
--enable-unaligned-access
--enable-static
--disable-xzdec
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-libtool-lock
