--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1,armthumb
--enable-match-finders=hc3,hc4
--enable-checks=crc32,crc64
--enable-threads=no
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-unsafe-type-punning
--disable-option-checking
--disable-xzdec
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-libtool-lock
--disable-nls
--disable-largefile
