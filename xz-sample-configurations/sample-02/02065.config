--enable-debug
--enable-encoders=lzma1,arm
--enable-decoders=lzma1,powerpc
--enable-match-finders=hc3,bt4
--enable-checks=crc32,sha256
--enable-small
--enable-threads=yes
--enable-shared=no
--enable-sandbox=no
--enable-silent-rules
