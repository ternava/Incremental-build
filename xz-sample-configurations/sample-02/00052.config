--enable-debug
--enable-encoders=lzma1,ia64
--enable-decoders=lzma1,ia64
--enable-match-finders=bt2
--enable-small
--enable-threads=yes
--enable-shared=no
--enable-sandbox=capsicum
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-assembler
--disable-xzdec
--disable-lzmainfo
--disable-scripts
--disable-libtool-lock
--disable-nls
