--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1,sparc
--enable-match-finders=hc4,hc3,bt3,bt4
--enable-checks=crc32
--enable-threads=yes
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-silent-rules
--enable-fast-install
--enable-fast-install=no
--disable-lzmadec
--disable-lzmainfo
--disable-scripts
--disable-dependency-tracking
--disable-libtool-lock
