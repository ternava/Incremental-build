--enable-debug
--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,powerpc
--enable-match-finders=hc4,hc3,bt2,bt3
--enable-checks=crc32,crc64,sha256
--enable-threads=yes
--enable-assume-ram=128
--enable-shared=no
--enable-dependency-tracking
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-xzdec
--disable-lzmadec
--disable-lzma-links
--disable-scripts
--disable-dependency-tracking
--disable-libtool-lock
