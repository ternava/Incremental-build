--enable-encoders=lzma1,ia64
--enable-decoders=lzma1,sparc
--enable-match-finders=hc3,bt2,bt3,bt4
--enable-checks=crc32,crc64
--enable-threads=posix
--enable-assume-ram=128
--enable-sandbox=no
--enable-fast-install
--enable-unaligned-access
--enable-static
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-silent-rules
--disable-rpath
