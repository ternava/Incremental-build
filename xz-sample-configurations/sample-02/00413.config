--enable-encoders=lzma1,arm
--enable-decoders=lzma1,ia64
--enable-match-finders=bt3
--enable-checks=crc32,crc64
--enable-threads=yes
--enable-assume-ram=128
--enable-symbol-versions
--enable-sandbox=auto
--enable-unaligned-access
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-scripts
--disable-silent-rules
--disable-nls
--disable-rpath
