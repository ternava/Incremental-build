--enable-debug
--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1,delta
--enable-match-finders=bt2,bt3,bt4
--enable-checks=crc32
--enable-threads=yes
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-dependency-tracking
--enable-fast-install
--enable-fast-install=no
--disable-lzmadec
--disable-lzma-links
--disable-silent-rules
--disable-dependency-tracking
--disable-nls
