--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,armthumb
--enable-match-finders=bt4
--enable-checks=crc32
--enable-small
--enable-threads=posix
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-fast-install
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-option-checking
--disable-lzmainfo
--disable-lzma-links
--disable-silent-rules
--disable-libtool-lock
