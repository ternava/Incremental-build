--enable-debug
--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,x86
--enable-match-finders=hc3,hc4
--enable-checks=crc32
--enable-threads=yes
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-silent-rules
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-option-checking
--disable-lzmadec
--disable-scripts
--disable-doc
--disable-silent-rules
--disable-libtool-lock
--disable-rpath
--disable-largefile
