--enable-encoders=lzma1
--enable-decoders=lzma1,delta
--enable-match-finders=bt2,bt3
--enable-checks=crc32
--enable-small
--enable-threads=yes
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-silent-rules
