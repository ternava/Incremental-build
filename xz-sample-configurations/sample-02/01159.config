--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1,lzma2
--enable-match-finders=bt2,bt3
--enable-checks=crc32
--enable-threads=yes
--enable-shared=no
--enable-sandbox=no
--enable-dependency-tracking
--enable-unaligned-access
--enable-static
--disable-lzmadec
--disable-scripts
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-rpath
