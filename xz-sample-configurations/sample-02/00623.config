--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,powerpc
--enable-match-finders=hc4,bt2,bt3,bt4
--enable-checks=crc32
--enable-threads=posix
--enable-assume-ram=128
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-fast-install
--enable-static
--disable-assembler
--disable-xzdec
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-libtool-lock
--disable-nls
