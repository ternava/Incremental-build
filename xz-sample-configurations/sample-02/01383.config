--enable-debug
--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1
--enable-match-finders=hc3
--enable-checks=crc32
--enable-threads=yes
--enable-shared=no
--enable-sandbox=no
--enable-silent-rules
--enable-fast-install
--enable-static
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
