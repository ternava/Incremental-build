--enable-encoders=lzma1,ia64
--enable-decoders=lzma1,x86
--enable-match-finders=hc4,bt4
--enable-checks=crc32,crc64,sha256
--enable-sandbox=no
--enable-fast-install
--enable-unsafe-type-punning
--disable-assembler
--disable-xzdec
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-silent-rules
--disable-rpath
