--enable-encoders=lzma1
--enable-decoders=lzma1,sparc
--enable-match-finders=hc4,hc3,bt2
--enable-checks=crc32
--enable-threads=posix
--enable-shared=no
--enable-sandbox=capsicum
--enable-unaligned-access
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-doc
--disable-silent-rules
--disable-rpath
