--enable-encoders=lzma1,lzma2,x86,armthumb,sparc,arm,ia64,powerpc,delta
--enable-decoders=lzma1,armthumb
--enable-match-finders=hc4,bt2,bt4
--enable-checks=crc32,crc64
--enable-threads=yes
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-dependency-tracking
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-doc
--disable-dependency-tracking
--disable-nls
