--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,arm
--enable-match-finders=hc4,bt2,bt4
--enable-checks=crc32,sha256
--enable-small
--enable-shared=no
--enable-sandbox=capsicum
--enable-silent-rules
