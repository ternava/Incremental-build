--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1
--enable-match-finders=hc4,bt3
--enable-checks=crc32
--enable-threads=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-dependency-tracking
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-lzmadec
--disable-scripts
--disable-libtool-lock
--disable-rpath
