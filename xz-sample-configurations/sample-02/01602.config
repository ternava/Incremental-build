--enable-debug
--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,powerpc
--enable-match-finders=hc4,bt2,bt3
--enable-checks=crc32
--enable-threads=posix
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-dependency-tracking
--enable-unaligned-access
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-lzmainfo
--disable-silent-rules
--disable-libtool-lock
--disable-nls
--disable-largefile
