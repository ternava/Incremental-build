--enable-debug
--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1
--enable-match-finders=hc4,hc3,bt2
--enable-checks=crc32
--enable-threads=yes
--enable-silent-rules
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-scripts
--disable-silent-rules
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-rpath
