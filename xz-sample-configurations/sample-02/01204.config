--enable-encoders=lzma1,delta
--enable-decoders=lzma1,sparc
--enable-match-finders=hc4,hc3,bt2
--enable-checks=crc32,sha256
--enable-threads=no
--enable-shared=no
--enable-sandbox=no
--enable-dependency-tracking
--enable-fast-install
--enable-unsafe-type-punning
--disable-option-checking
--disable-lzmadec
--disable-lzmainfo
--disable-silent-rules
--disable-libtool-lock
--disable-rpath
