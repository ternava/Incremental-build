--enable-debug
--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,lzma2
--enable-match-finders=hc4,bt2,bt3
--enable-checks=crc32,sha256
--enable-small
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-unaligned-access
--disable-lzmadec
--disable-lzmainfo
--disable-scripts
--disable-doc
--disable-libtool-lock
--disable-nls
--disable-largefile
