--enable-encoders=lzma1,x86
--enable-decoders=lzma1,armthumb
--enable-match-finders=hc3,bt2,bt3
--enable-checks=crc32
--enable-threads=yes
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-fast-install
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-xzdec
--disable-lzmainfo
--disable-silent-rules
--disable-libtool-lock
--disable-nls
--disable-largefile
