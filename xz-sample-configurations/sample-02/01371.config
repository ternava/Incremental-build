--enable-debug
--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1,sparc
--enable-match-finders=bt3,bt4
--enable-checks=crc32
--enable-small
--enable-threads=yes
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=no
--enable-silent-rules
--enable-dependency-tracking
--enable-unsafe-type-punning
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-doc
--disable-dependency-tracking
--disable-libtool-lock
--disable-rpath
