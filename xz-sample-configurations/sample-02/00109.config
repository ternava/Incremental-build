--enable-debug
--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,lzma2,delta,x86,sparc,ia64,armthumb,arm,powerpc
--enable-match-finders=hc4,hc3,bt2,bt3
--enable-checks=crc32,sha256
--enable-threads=posix
--enable-assume-ram=128
--enable-symbol-versions
--enable-sandbox=auto
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-silent-rules
--disable-nls
