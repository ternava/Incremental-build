--enable-debug
--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,armthumb
--enable-match-finders=hc3,hc4
--enable-checks=crc32,sha256
--enable-small
--enable-threads=posix
--enable-sandbox=auto
--enable-unaligned-access
--enable-fast-install=no
--disable-assembler
--disable-lzmadec
--disable-lzmainfo
--disable-scripts
--disable-silent-rules
--disable-dependency-tracking
