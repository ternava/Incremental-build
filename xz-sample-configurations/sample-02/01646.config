--enable-encoders=lzma1,delta
--enable-decoders=lzma1,powerpc
--enable-match-finders=bt2
--enable-checks=crc32,sha256
--enable-threads=no
--enable-shared=no
--enable-sandbox=no
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install
--enable-unsafe-type-punning
--disable-option-checking
--disable-xzdec
--disable-lzmadec
--disable-lzma-links
--disable-doc
--disable-libtool-lock
--disable-nls
--disable-largefile
