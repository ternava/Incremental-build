--enable-debug
--enable-encoders=lzma1
--enable-decoders=lzma1,armthumb
--enable-match-finders=hc3,bt2,bt4
--enable-checks=crc32,sha256
--enable-small
--enable-threads=posix
--enable-sandbox=no
--enable-dependency-tracking
--enable-unaligned-access
--enable-fast-install=no
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-largefile
