--enable-encoders=lzma1
--enable-decoders=lzma1,x86
--enable-match-finders=hc4,hc3,bt2
--enable-checks=crc32,crc64,sha256
--enable-threads=no
--enable-assume-ram=128
--enable-sandbox=no
--enable-silent-rules
--enable-unaligned-access
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzmainfo
--disable-doc
--disable-dependency-tracking
--disable-libtool-lock
