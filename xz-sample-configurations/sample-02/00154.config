--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1,sparc
--enable-match-finders=hc3,hc4
--enable-checks=crc32,crc64
--enable-small
--enable-threads=posix
--enable-assume-ram=128
--enable-shared=no
--enable-sandbox=auto
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-scripts
--disable-doc
--disable-dependency-tracking
--disable-nls
--disable-rpath
--disable-largefile
