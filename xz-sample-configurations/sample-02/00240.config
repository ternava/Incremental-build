--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1,ia64
--enable-match-finders=hc3
--enable-checks=crc32
--enable-threads=posix
--enable-shared=no
--enable-symbol-versions
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-xzdec
--disable-lzmainfo
--disable-scripts
--disable-doc
--disable-silent-rules
--disable-libtool-lock
--disable-rpath
--disable-largefile
