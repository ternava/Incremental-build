--enable-debug
--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,ia64
--enable-match-finders=hc4
--enable-checks=crc32
--enable-threads=yes
--enable-symbol-versions
--enable-sandbox=no
--enable-dependency-tracking
--enable-unaligned-access
--disable-option-checking
--disable-assembler
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-doc
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
