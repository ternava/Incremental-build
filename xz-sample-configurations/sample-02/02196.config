--enable-encoders=lzma1,delta
--enable-decoders=lzma1,x86
--enable-match-finders=bt4
--enable-checks=crc32,crc64
--enable-small
--enable-sandbox=auto
--enable-fast-install
--enable-unaligned-access
--enable-fast-install=no
--disable-option-checking
--disable-lzmainfo
--disable-lzma-links
--disable-silent-rules
--disable-rpath
