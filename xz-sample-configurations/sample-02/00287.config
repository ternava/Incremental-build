--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,x86
--enable-match-finders=bt2,bt3
--enable-checks=crc32,sha256
--enable-assume-ram=128
--enable-shared=no
--enable-silent-rules
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-xzdec
--disable-lzmadec
--disable-lzma-links
--disable-doc
--disable-dependency-tracking
--disable-nls
--disable-rpath
