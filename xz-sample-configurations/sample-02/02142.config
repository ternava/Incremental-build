--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1,delta
--enable-match-finders=bt3,bt4
--enable-checks=crc32
--enable-threads=no
--enable-assume-ram=128
--enable-shared=no
--enable-sandbox=capsicum
--enable-silent-rules
--enable-unaligned-access
--enable-fast-install=no
--disable-xzdec
--disable-lzma-links
--disable-dependency-tracking
