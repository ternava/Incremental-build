--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1,x86
--enable-match-finders=hc4,hc3,bt2,bt3
--enable-checks=crc32,crc64
--enable-threads=posix
--enable-assume-ram=128
--enable-sandbox=capsicum
--enable-dependency-tracking
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-scripts
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
