--enable-debug
--enable-encoders=lzma1
--enable-decoders=lzma1,x86
--enable-match-finders=bt2,bt3,bt4
--enable-checks=crc32,crc64,sha256
--enable-threads=posix
--enable-sandbox=no
--enable-unaligned-access
--enable-static
--enable-fast-install=no
--disable-xzdec
--disable-lzmainfo
--disable-scripts
--disable-doc
--disable-silent-rules
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
