--enable-debug
--enable-encoders=lzma1,arm
--enable-decoders=lzma1,ia64
--enable-match-finders=bt4
--enable-checks=crc32,sha256
--enable-small
--enable-threads=yes
--enable-sandbox=no
--enable-silent-rules
--enable-dependency-tracking
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--disable-xzdec
--disable-lzmainfo
--disable-lzma-links
--disable-doc
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
