--enable-debug
--enable-encoders=lzma1,delta
--enable-decoders=lzma1,arm
--enable-match-finders=hc4,hc3,bt4
--enable-checks=crc32,crc64
--enable-small
--enable-threads=yes
--enable-symbol-versions
--enable-silent-rules
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--disable-xzdec
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-nls
--disable-largefile
