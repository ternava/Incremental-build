--enable-encoders=lzma1,delta
--enable-decoders=lzma1
--enable-match-finders=hc3,bt2,bt3
--enable-small
--enable-threads=posix
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
