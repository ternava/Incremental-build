--enable-encoders=lzma1
--enable-decoders=lzma1,delta
--enable-match-finders=hc3,bt2,bt4
--enable-checks=crc32,crc64
--enable-small
--enable-sandbox=auto
--enable-silent-rules
--enable-unsafe-type-punning
--disable-option-checking
--disable-lzma-links
--disable-silent-rules
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-largefile
