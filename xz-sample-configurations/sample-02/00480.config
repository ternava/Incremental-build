--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,delta
--enable-match-finders=bt2
--enable-checks=crc32,crc64
--enable-threads=no
--enable-shared=no
--enable-symbol-versions
--enable-silent-rules
--enable-fast-install=no
--disable-option-checking
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-nls
