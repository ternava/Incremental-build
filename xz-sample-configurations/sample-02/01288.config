--enable-debug
--enable-encoders=lzma1
--enable-decoders=lzma1,arm
--enable-match-finders=bt2,bt3
--enable-checks=crc32
--enable-small
--enable-symbol-versions
--enable-sandbox=no
--enable-static
--disable-option-checking
--disable-lzmadec
--disable-doc
--disable-silent-rules
--disable-dependency-tracking
--disable-libtool-lock
--disable-rpath
