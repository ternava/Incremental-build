--enable-encoders=lzma1
--enable-decoders=lzma1,x86
--enable-match-finders=hc3,bt2,bt3
--enable-checks=crc32
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-unsafe-type-punning
--enable-fast-install=no
--disable-lzmadec
--disable-scripts
--disable-dependency-tracking
--disable-libtool-lock
--disable-rpath
