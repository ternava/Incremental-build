--enable-debug
--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1,ia64
--enable-match-finders=hc4,bt2,bt3
--enable-checks=crc32
--enable-threads=no
--enable-shared=no
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install
--enable-unaligned-access
--disable-assembler
--disable-lzmadec
--disable-lzma-links
--disable-nls
--disable-rpath
