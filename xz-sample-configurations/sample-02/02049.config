--enable-encoders=lzma1,x86
--enable-decoders=lzma1
--enable-match-finders=hc3,hc4
--enable-checks=crc32
--enable-threads=yes
--enable-shared=no
--enable-sandbox=no
--enable-silent-rules
--enable-fast-install
--enable-unsafe-type-punning
--enable-static
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
