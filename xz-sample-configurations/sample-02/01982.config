--enable-encoders=lzma1,ia64
--enable-decoders=lzma1
--enable-match-finders=hc4,bt2
--enable-checks=crc32,crc64
--enable-threads=posix
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-silent-rules
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--disable-option-checking
--disable-assembler
--disable-lzmadec
--disable-scripts
--disable-dependency-tracking
--disable-nls
--disable-rpath
--disable-largefile
