--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1,ia64
--enable-match-finders=bt2,bt3,bt4
--enable-checks=crc32
--enable-small
--enable-assume-ram=128
--enable-silent-rules
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--disable-assembler
--disable-lzma-links
--disable-doc
--disable-silent-rules
--disable-libtool-lock
--disable-rpath
