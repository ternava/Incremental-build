--enable-debug
--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,arm
--enable-match-finders=hc4,bt2,bt3
--enable-checks=crc32,crc64
--enable-small
--enable-threads=posix
--enable-assume-ram=128
--enable-shared=no
--enable-sandbox=capsicum
--enable-dependency-tracking
--enable-fast-install
--enable-static
--enable-fast-install=no
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-silent-rules
