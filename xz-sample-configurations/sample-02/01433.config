--enable-debug
--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,ia64
--enable-match-finders=hc4,hc3,bt2,bt3
--enable-checks=crc32,crc64
--enable-small
--enable-assume-ram=128
--enable-shared=no
--enable-dependency-tracking
--enable-unaligned-access
--disable-lzmadec
--disable-lzma-links
--disable-dependency-tracking
--disable-libtool-lock
--disable-largefile
