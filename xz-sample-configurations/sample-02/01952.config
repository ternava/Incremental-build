--enable-debug
--enable-encoders=lzma1,sparc
--enable-decoders=lzma1
--enable-match-finders=hc4
--enable-checks=crc32,crc64
--enable-small
--enable-threads=yes
--enable-assume-ram=128
--enable-symbol-versions
--enable-silent-rules
--enable-dependency-tracking
--enable-unaligned-access
--enable-static
--disable-xzdec
--disable-lzmadec
--disable-lzma-links
--disable-nls
--disable-rpath
--disable-largefile
