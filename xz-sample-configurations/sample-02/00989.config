--enable-debug
--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1,delta
--enable-match-finders=hc4,bt2,bt4
--enable-checks=crc32,sha256
--enable-small
--enable-threads=posix
--enable-shared=no
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-lzmadec
--disable-doc
--disable-dependency-tracking
--disable-largefile
