--enable-debug
--enable-encoders=lzma1
--enable-decoders=lzma1,powerpc
--enable-match-finders=hc4,hc3,bt2
--enable-checks=crc32
--enable-threads=posix
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-silent-rules
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-silent-rules
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-rpath
