--enable-encoders=lzma1,delta
--enable-decoders=lzma1
--enable-match-finders=hc3,bt3
--enable-checks=crc32
--enable-threads=yes
--enable-assume-ram=128
--enable-shared=no
--enable-sandbox=capsicum
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-lzmadec
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-rpath
