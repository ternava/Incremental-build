--enable-debug
--enable-encoders=lzma1
--enable-decoders=lzma1,sparc
--enable-match-finders=bt2,bt3
--enable-checks=crc32,crc64
--enable-threads=yes
--enable-sandbox=no
--enable-dependency-tracking
--enable-fast-install
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-scripts
--disable-doc
--disable-silent-rules
--disable-nls
--disable-rpath
--disable-largefile
