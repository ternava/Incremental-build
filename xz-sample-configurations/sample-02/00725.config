--enable-debug
--enable-encoders=lzma1,arm
--enable-decoders=lzma1,lzma2
--enable-match-finders=hc3,bt3,bt4
--enable-checks=crc32,crc64
--enable-threads=no
--enable-assume-ram=128
--enable-sandbox=capsicum
--enable-dependency-tracking
--enable-static
--enable-fast-install=no
--disable-lzmadec
--disable-doc
--disable-libtool-lock
--disable-nls
