--enable-debug
--enable-encoders=lzma1,delta
--enable-decoders=lzma1,armthumb
--enable-match-finders=bt4
--enable-checks=crc32,sha256
--enable-threads=no
--enable-assume-ram=128
--enable-symbol-versions
--enable-fast-install
--enable-unsafe-type-punning
--disable-option-checking
--disable-xzdec
--disable-doc
--disable-silent-rules
--disable-dependency-tracking
--disable-rpath
