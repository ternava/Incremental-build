--enable-debug
--enable-encoders=lzma1,ia64
--enable-decoders=lzma1,arm
--enable-match-finders=bt2
--enable-checks=crc32,crc64,sha256
--enable-threads=no
--enable-sandbox=capsicum
--enable-silent-rules
--enable-fast-install
--enable-unaligned-access
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-lzma-links
--disable-scripts
--disable-libtool-lock
--disable-rpath
