--enable-debug
--enable-encoders=lzma1,arm
--enable-decoders=lzma1,delta
--enable-match-finders=hc4,bt3,bt4
--enable-checks=crc32,crc64,sha256
--enable-small
--enable-threads=yes
--enable-symbol-versions
--enable-sandbox=auto
--enable-static
--disable-assembler
--disable-lzmadec
--disable-scripts
--disable-doc
--disable-libtool-lock
--disable-nls
--disable-rpath
--disable-largefile
