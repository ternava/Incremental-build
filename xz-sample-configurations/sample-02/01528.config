--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1,arm
--enable-match-finders=hc4,bt2,bt4
--enable-checks=crc32,crc64,sha256
--enable-small
--enable-threads=yes
--enable-sandbox=auto
--enable-fast-install
--enable-unaligned-access
--enable-static
--disable-assembler
--disable-xzdec
--disable-lzma-links
--disable-silent-rules
--disable-libtool-lock
