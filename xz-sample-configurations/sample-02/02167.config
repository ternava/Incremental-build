--enable-debug
--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,ia64
--enable-match-finders=hc4,hc3,bt2,bt4
--enable-checks=crc32,crc64,sha256
--enable-sandbox=capsicum
--enable-silent-rules
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--disable-option-checking
--disable-assembler
--disable-lzmadec
--disable-lzma-links
--disable-doc
--disable-libtool-lock
--disable-rpath
--disable-largefile
