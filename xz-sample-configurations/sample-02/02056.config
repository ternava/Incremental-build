--enable-encoders=lzma1,ia64
--enable-decoders=lzma1,ia64
--enable-match-finders=bt2,bt3
--enable-checks=crc32
--enable-small
--enable-threads=posix
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-dependency-tracking
--enable-unaligned-access
--enable-unsafe-type-punning
--disable-dependency-tracking
--disable-largefile
